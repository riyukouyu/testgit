<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="ItemType" kind="business" longname="科目相关服务定义" package="cn.sunline.ltts.busi.in.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[科目相关服务定义]]></description>
    <service id="QryItemInfo" name="qryItemInfo" longname="分页查询科目信息">
        <interface>
            <input packMode="true">
                <field id="startid" type="int" required="false" multi="false" array="false" longname="起始记录"/>
                <field id="recdct" type="int" required="false" multi="false" array="false" longname="页容量"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="itemInfo" type="InItemInfo.ItemInfos" required="false" multi="false" array="false" longname="科目信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="InsItemInfo" name="insItemInfo" longname="新增科目">
        <interface>
            <input packMode="false">
                <field id="inputItem" type="InItemInfo.ItemInfo" required="false" multi="false" array="false" longname="科目信息"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="UptItemInfo" name="uptItemInfo" longname="科目维护">
        <interface>
            <input packMode="false">
                <field id="itemcd" type="BaseType.U_ITEMCD" required="false" multi="false" array="false" longname="科目" ref="InDict.inac.itemcd"/>
                <field id="itemna" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="科目名称" ref="InDict.inac.itemna"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
