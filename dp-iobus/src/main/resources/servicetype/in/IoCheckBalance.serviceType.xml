<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoCheckBalance" kind="business" longname="检查交易平衡性服务定义" package="cn.sunline.ltts.busi.iobus.servicetype.in" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[检查交易平衡性服务定义]]></description>
    <service id="CheckBalance" name="checkBalance" longname="检验平衡性">
        <interface>
            <input packMode="false">
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="InDict.inac.trandt"/>
                <field id="transq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水" ref="DpDict.Acct.transq"/>
                <field id="clactp" type="BaseEnumType.E_CLACTP" required="false" multi="false" array="false" longname="清算账户类型"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="StrikeCheckBalanceClear" name="strikeCheckBalanceClear" longname="平衡检查中的清算补账冲正">
        <interface>
            <input packMode="false">
                <field id="checkstrike" type="IoInWriteOffComplex.IoCheckBlanaceStrike" required="false" multi="false" array="false" longname="平衡检查中的清算补账冲正"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="GetGlKnaAcct" name="getGlKnaAcct" longname="获取内部户账号信息">
        <interface>
            <input packMode="false">
                <field id="busino" type="BaseType.U_BUSINO" required="false" multi="false" array="false" longname="业务编号"/>
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" ref="BaseDict.Comm.crcycd" required="false" multi="false" array="false" longname="币种"/>
                <field id="subsac" type="BaseType.U_CHAR16" required="false" multi="false" array="false" longname="子户号"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="tblGlKnaAcct" type="IoInTable.IoGlKnaAcct" required="false" multi="false" array="false" longname="内部户信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="CheckBalanceLsam" name="checkBalanceLsam" longname="大小额来往帐及冲正平衡性检查">
        <interface>
            <input packMode="false">
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="InDict.inac.trandt"/>
                <field id="transq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水" ref="DpDict.Acct.transq"/>
                <field id="clactp" type="BaseEnumType.E_CLACTP" required="false" multi="false" array="false" longname="清算账户类型"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
