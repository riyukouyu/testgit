<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoInSrvQryTableInfo" kind="business" longname="查询内部户相关表信息" package="cn.sunline.ltts.busi.iobus.servicetype.in" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="IoSelGlKnaAcctnoByAcctno" name="selKnaGlAcctnoByAcctno" longname="根据内部账号查询账户信息">
        <interface>
            <input packMode="false">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="内部户账号" ref="InDict.inac.acctno"/>
                <field id="epflag" type="boolean" required="false" multi="false" array="false" longname="抛异常标志"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="cplGlknac" type="IoInQueryTypes.IoInacInfo" required="false" multi="false" array="false" longname="内部户信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="selGlKnsGlvcByTransq" name="selGlKnsGlvcByTransq" longname="根据交易流水查询传票流水表">
        <interface>
            <input packMode="false">
                <field id="transq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="交易流水" ref="BaseDict.Comm.transq"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="ioGlKnsGlvc" type="IoInTable.IoGlKnsGlvc" required="false" multi="false" array="false" longname="内部户信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
