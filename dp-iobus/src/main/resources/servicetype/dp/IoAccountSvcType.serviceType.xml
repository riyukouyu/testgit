                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
                <field id="sortno" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="记录序号"/>
                <field id="acctna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="账户名称"/>
