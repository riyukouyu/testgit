进行财务平衡性检查]]></description>
    <service id="IoAccountting" name="ioAccountting" longname="登记会计流水">
        <interface>
            <input packMode="false">
                <field id="cplIoAccounttingIntf" type="IoAccountComplexType.IoAccounttingIntf" required="false" multi="false" array="false" longname="记账接口"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoQueryKnsAcsq" name="queryKnsAcsq" longname="查询会计记账流水信息">
        <interface>
            <input packMode="false">
                <field id="bgindt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始交易日期"/>
                <field id="endddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="终止交易日期"/>
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="账号"/>
                <field id="transq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水" ref="DpDict.Acct.transq"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人代码" ref="ApDict.Aplt.corpno"/>
                <field id="pageno" type="long" required="false" multi="false" array="false" longname="页码"/>
                <field id="pgsize" type="long" required="false" multi="false" array="false" longname="页容量"/>
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="knsAcsqInfoList" type="IoAccountComplexType.IoKnsAcsqInfo" required="false" multi="true" array="false" longname="会计记账流水信息集合"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoRegistKnsAcsqCler" name="registKnsAcsqCler" longname="登记会计流水清算信息">
        <interface>
            <input packMode="false">
                <field id="clearinfo" type="IoAccountComplexType.IoAccountClearInfo" required="false" multi="false" array="false" longname="会计流水清算信息"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoQueryKnsAcsqCler" name="queryKnsAcsqCler" longname="查询会计流水清算信息">
        <interface>
            <input packMode="false">
