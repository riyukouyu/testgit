<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="IoDpStaPublic" kind="business" longname="提供冻结查询信息服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[冻结信息对外提供方法在此定义]]></description>
    <service id="KnbFroz_getFrozstByCustac" name="KnbFroz_getFrozstByCustac" longname="获取电子账号的冻结状态">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="客户账号" ref="PbDict.Charge.custac"/>
                <field id="isable" type="boolean" required="false" multi="false" array="false" longname="是否抛出异常"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="frozst" type="DpEnumType.E_FROZST" required="false" multi="false" array="false" longname="冻结状态" ref="DpDict.Froz.frozst"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="KnbFroz_getInfoByCustac" name="KnbFroz_getInfoByCustac" longname="通过电子账号获取冻结信息">
        <interface>
            <input packMode="false">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.custac"/>
                <field id="isable" type="boolean" required="false" multi="false" array="false" longname="是否抛出异常"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="knaFroz" type="IoDpTable.IoDpKnbFroz" required="false" multi="true" array="false" longname="冻结登记薄表复合类型集合"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="DoFrozrByMntrsqdt" name="DoFrozByMntrsqdt" longname="根据冻结交易流水和交易日期冻结">
        <interface>
            <input packMode="false">
                <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期"/>
                <field id="isable" type="boolean" required="false" multi="false" array="false" longname="是否抛出异常"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="DoUnFrByMntrsqdt" name="DoUnFrByMntrsqdt" longname="根据冻结交易流水和交易日期解冻">
        <interface>
            <input packMode="false">
                <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期"/>
                <field id="isable" type="boolean" required="false" multi="false" array="false" longname="是否抛出异常"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoDpCheckAcCarry" name="checkAcCarry" longname="检查电子账户是否可以转入转出">
        <interface>
            <input packMode="true">
                <field id="cardno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="tranam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="交易金额"/>
                <field id="trantp" type="CaEnumType.E_CKTRTP" required="false" multi="false" array="false" longname="交易类型" desc="01-ATM存现02-ATM取现03-ATM转入04-银联在线转入05-银联CUPS转入06-本行借记卡转入07-本行贷记卡转入"/>
                <field id="toacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="对方账号"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="isable" type="CaEnumType.E_CKTRIF" required="false" multi="false" array="false" longname="是否允许转账" desc="01-允许 02-不允许 03-账户不存在"/>
                <field id="custnm" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="客户名称"/>
                <field id="remark" type="BaseType.U_SMRYDS" required="false" multi="false" array="false" longname="描述"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
