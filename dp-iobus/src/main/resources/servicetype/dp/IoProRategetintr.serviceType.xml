<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoProRategetintr" kind="business" longname="基础产品获取利率相关信息" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[基础产品获取利率相关信息]]></description>
    <service id="IoProRateSelinfo" name="IoProRateSelinfo" longname="根据产品编码查询税率编码">
        <interface>
            <input packMode="false">
                <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编号" ref="DpDict.Prod.prodcd"/>
            </input>
            <output asParm="false" packMode="true">
                <field id="intrcd" type="BaseType.U_INRTNO" required="false" multi="false" array="false" longname="利率编号" ref="DpDict.Prod.intrcd"/>
                <field id="incdtp" type="DpEnumType.E_IRCDTP" required="false" multi="false" array="false" longname="利率代码类型" ref="DpDict.Prod.incdtp"/>
                <field id="taxecd" type="BaseType.U_TXRTNO" required="false" multi="false" array="false" longname="税率编号" ref="DpDict.Prod.taxecd"/>
                <field id="intrwy" type="DpEnumType.E_IRDPWY" required="false" multi="false" array="false" longname="利率靠档方式" ref="DpDict.Prod.intrwy"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoProInfoByProdno" name="IoProInfoByProdno" longname="查询装配产品信息">
        <interface>
            <input packMode="false">
                <field id="prodcd" type="BaseType.U_PRODCD" required="true" multi="false" array="false" longname="产品编号" ref="DpDict.Prod.prodcd"/>
                <field id="corpno" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="法人号"/>
            </input>
            <output asParm="false" packMode="false">
                <field id="proInfo" type="IoProRateSel.ProInfoAll" required="false" multi="false" array="false" longname="装配产品信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
