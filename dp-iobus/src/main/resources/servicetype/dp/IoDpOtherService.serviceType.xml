<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoDpOtherService" kind="business" longname="交易外调服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[交易外调服务]]></description>
    <service id="IoDpSentMaxIntrvlMsg" name="dpSentMaxIntrvlMsg" longname="发送利率编号及最大值给移动前端">
        <interface>
            <input packMode="true">
                <field id="msgid" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="发送消息ID"/>
                <!-- field id="medium" type="ECustPlugin.E_MEDIUM" required="false" multi="false" array="false" longname="消息媒介"/-->
                <field id="mdname" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="媒介名称"/>
                <field id="intinf" type="IoIntrComplexType.IoMaxir" required="false" multi="false" array="false" longname="返回明细"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoDpSendFrozMsg" name="sendFrozMsg" longname="账户保护发送通知给移动前端">
        <interface>
            <input packMode="true">
                <field id="msgid" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="发送消息ID"/>
                <!-- field id="medium" type="ECustPlugin.E_MEDIUM" required="false" multi="false" array="false" longname="消息媒介"/-->
                <field id="mdname" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="媒介名称"/>
                <field id="frzinf" type="IoDpFrozComplexType.IoFrozMsgInfo" required="false" multi="false" array="false" longname="返回明细"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="IoDpSendSignMsg" name="sendSignMsg" longname="存款签约发送通知给合约库">
        <interface>
            <input packMode="true">
                <field id="msgid" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="发送消息ID"/>
                <!-- field id="medium" type="ECustPlugin.E_MEDIUM" required="false" multi="false" array="false" longname="消息媒介"/-->
                <field id="mdname" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="媒介名称"/>
                <field id="signst" type="CaEnumType.E_SIGNST" required="false" multi="false" array="false" longname="签约状态"/>
                <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号"/>
                <field id="cardno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="custna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="客户名称" ref="DpDict.Acct.custna"/>
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="开户机构号"/>
                <field id="brchna" type="BaseType.U_BRCHNA" required="false" multi="false" array="false" longname="机构名称"/>
                <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编码"/>
                <field id="prodna" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="产品名称"/>
                <field id="keepam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="留存金额"/>
                <field id="trmiam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="起存金额"/>
                <field id="timetm" type="ApBaseType.U_TMSTMP" required="false" multi="false" array="false" longname="时间戳" ref="ApDict.Aplt.timetm"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期"/>
                <field id="signdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="签约日期"/>
                <field id="effedt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="失效日期"/>
                <field id="tranbr" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="交易机构"/>
                <field id="servtp" type="MsType.U_CHANNEL" required="false" multi="false" array="false" longname="渠道"/>
                <field id="fcflag" type="DpEnumType.E_FCFLAG" required="false" multi="false" array="false" longname="定活标志"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IoDpBusiCheck" name="dpBusiCheck" longname="调用客户认证系统客户认证">
        <interface>
            <input packMode="true">
                <field id="sessionId" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="sessionId"/>
                <field id="userId" type="BaseType.U_CHAR14" required="false" multi="false" array="false" longname="用户ID"/>
                <field id="authComb" type="IoDpCapital.CapAuthComb" required="false" multi="true" array="false" longname="认证组合"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
