<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoCaSevSmsManager" kind="business" longname="电子账户短信功能管理服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="SmsOpenClose" name="smsOpenClose" longname="设置是否开通短信通知">
        <interface>
            <input packMode="true">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.custac"/>
                <field id="teleno" type="BaseType.U_TELENO" required="false" multi="false" array="false" longname="电话号码" ref="DpDict.Acct.teleno"/>
            	<field id="isopen" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否开通短信服务"/>
            </input>
            <output asParm="false" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="QrySmsStatus" name="qrySmsStatus" longname="查询是否开通短信通知">
        <interface>
            <input packMode="true">
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.custac"/>
                <field id="teleno" type="BaseType.U_TELENO" required="false" multi="false" array="false" longname="电话号码" ref="DpDict.Acct.teleno"/>
            </input>
            <output asParm="true" packMode="true">
            	<field id="isopen" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否开通短信服务"/>    
			</output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    
</serviceType>
