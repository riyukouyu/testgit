<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoDpJointSvc" kind="business" longname="联名卡相关服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <service id="AddJointAccount" name="addJointAccount" longname="新增联名账户">
        <interface>
            <input packMode="false">
                <field id="jointa" type="IoDpJointType.IoDpKnaAcctJointInfo" required="false" multi="false" array="false" longname="联名账户信息"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="joinac" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="联名账号"/>
                <field id="fcflag" type="DpEnumType.E_FCFLAG" required="false" multi="false" array="false" longname="活定标志"/>
                <field id="prodtp" type="DpEnumType.E_PRODTP" required="false" multi="false" array="false" longname="产品类型"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="DpDict.Acct.crcycd"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="AddJointLimit" name="addJointLimit" longname="新增联名账户额度信息">
        <interface>
            <input packMode="false">
                <field id="joinli" type="IoDpJointType.IoDpKnaAcctJointLimitInfo" required="false" multi="false" array="false" longname="联名账户额度信息"/>
            </input>
            <output asParm="true" packMode="true"/>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="UpdJointAcctBalance" name="updJointAcctBalance" longname="联名卡更改账户余额">
        <interface>
            <input packMode="false">
                <field id="joinin" type="IoDpJointType.IoDpJointAcctBalanceIn" required="false" multi="false" array="false" longname="联名账户信息"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="acctbl" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="交易后金额"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="SelAggregateAmountByTrandt" name="selAggregateAmountByTrandt" longname="根据日期查询汇总消费明细">
        <interface>
            <input packMode="false">
                <field id="platno" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="平台编号"/>
                <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="BaseDict.Comm.trandt"/>
                <field id="jotrtp" type="DpEnumType.E_JOTRTP" required="false" multi="false" array="false" longname="联名交易类型"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="aggram" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="汇总消费金额"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
