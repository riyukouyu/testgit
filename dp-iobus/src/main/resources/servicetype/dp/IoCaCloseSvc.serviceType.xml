<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="IoCaCloseSvc" kind="business" longname="电子账户销户服务" package="cn.sunline.ltts.busi.iobus.servicetype.dp" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
