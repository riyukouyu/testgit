            </in_mappings>
            <out_mappings by_interface="false">
                <mapping src="brchInfo.brchno" dest="brchno" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <method method="dlsedt" id="dlsedt" longname="删除场景代码明细管理" desc="删除场景代码明细管理"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
