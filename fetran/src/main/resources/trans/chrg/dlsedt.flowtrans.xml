<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="dlsedt" longname="删除场景代码明细管理" kind="2" package="cn.sunline.ltts.busi.fetran.trans.chrg" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[场景事件明细信息删除]]></description>
    <interface package="cn.sunline.ltts.busi.fetran.transdef.chrg">
