<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="adsedt" longname="新增场景代码明细表" kind="2" package="cn.sunline.ltts.busi.fetran.trans.chrg" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.fetran.trans.chrg.intf">
        <input packMode="true">
            <field id="scends" type="FeBaseType.U_LONGDS" required="true" multi="false" array="false" longname="场景名称" ref="FeDict.Chrg.scends"/>
            <field id="evetcd" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="事件编号" ref="FeDict.Chrg.evetcd"/>
            <field id="remark" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="备注信息" ref="FeDict.Chrg.remark"/>
            <fields id="sedtif" required="false" multi="true" longname="场景代码明细列表" array="false">
                <field id="dimecg" type="FeBaseType.U_DIMECG" required="false" multi="false" array="false" longname="维度类别" ref="FeDict.Chrg.dimecg"/>
                <field id="dimevl" type="BaseType.U_CHAR32" required="true" multi="false" array="false" longname="维度值" ref="FeDict.Chrg.dimevl"/>
            </fields>
        </input>
        <output asParm="true" packMode="true">
            <field id="scencd" type="FeBaseType.U_SCENCD" required="false" multi="false" array="false" longname="场景代码" ref="FeDict.Chrg.scencd"/>
        </output>
        <property packMode="true">
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="BaseDict.Comm.brchno"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service mappingToProperty="true" serviceName="IoSrvPbBranch.IoGetCenterBranch" id="IoGetCenterBranch" longname="获取中心机构清算中心">
            <in_mappings by_interface="true">
                <mapping src="#run.tranbr" dest="brchno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="false">
                <mapping src="brchInfo.brchno" dest="brchno" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <method method="adsedt" id="adsedt" longname="新增场景代码明细表" desc="新增场景代码明细表"/>
    </flow>
</flowtran>
