<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="adcmdt" longname="新增计费公式明细" kind="2" package="cn.sunline.ltts.busi.fetran.trans.chrg" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.fetran.trans.chrg.intf">
        <input packMode="true">
            <fields id="listfm" required="false" multi="true" longname="公式列表" array="false">
                <field id="chrgfm" type="FeBaseType.U_FOMUCD" required="false" multi="false" array="false" longname="计费公式代码" ref="FeDict.Chrg.chrgfm"/>
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="PbDict.Brch.brchno"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
                <field id="limiam" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="区间金额下限"/>
                <field id="cufetp" type="FeEnumType.E_CUFETP" required="false" multi="false" array="false" longname="计费类型" ref="FeDict.Chrg.cufetp"/>
                <field id="chrgrt" type="FeBaseType.U_RATVAL" required="false" multi="false" array="false" longname="收费比例" ref="FeDict.Chrg.chrgrt"/>
                <field id="pecgam" type="FeBaseType.U_TOTLAM" required="false" multi="false" array="false" longname="计费单价" ref="FeDict.Chrg.pecgam"/>
                <field id="cgmnam" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="最低金额" ref="FeDict.Chrg.cgmnam"/>
                <field id="cgmxam" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="最高金额" ref="FeDict.Chrg.cgmxam"/>
                <field id="remark" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="备注信息" ref="FeDict.Chrg.remark"/>
            </fields>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="BaseDict.Comm.brchno"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service mappingToProperty="true" serviceName="IoSrvPbBranch.IoGetCenterBranch" id="IoGetCenterBranch" longname="获取中心机构清算中心">
            <in_mappings by_interface="true">
                <mapping src="#run.tranbr" dest="brchno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="false">
                <mapping src="brchInfo.brchno" dest="brchno" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <method method="adcmdt" id="adcmdt" longname="新增计费公式明细表" desc="新增计费公式明细表"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
