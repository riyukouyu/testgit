<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrcglt" longname="费用清单报表查询" kind="1" package="cn.sunline.ltts.busi.cgtran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[]]></description>
    <interface package="cn.sunline.ltts.busi.cgtran.trans.intf">
        <input packMode="true">
            <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号" ref="BaseDict.Comm.custno"/>
            <field id="custac" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="客户账号" ref="BaseDict.Comm.custac"/>
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="BaseDict.Comm.brchno"/>
            <field id="recvfg" type="BaseEnumType.E_GTAMFG" required="false" multi="false" array="false" longname="收费状态"/>
            <field id="chrgcy" type="BaseType.U_CRCYCD" required="false" fixed="" default="" multi="false" array="false" longname="币种" desc="" ref=""/>
            <field id="servtp" type="MsType.U_CHANNEL" required="false" fixed="" default="" multi="false" array="false" longname="交易渠道" desc="" ref=""/>
            <field id="bgdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始日期"/>
            <field id="endate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="截止日期"/>
            <field id="cgpyrv" type="PbEnumType.E_CGPYRV" required="false" multi="false" array="false" longname="费用收付标志" ref="PbDict.Charge.cgpyrv"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="disChargeInfo" type="IoCgChrg.disChargeInfo" required="false" multi="true" array="false" longname="费用清单报表输出"/>
        </output>
        <property packMode="true">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页码"/>
            <field id="pagesz" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页容量"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow id="" test="" longname="" desc="">
        <service serviceName="IoCgChrgSvcType.IoCgChageList" id="IoCgChageList" test="" longname="费用清单报表查询" desc="费用清单报表查询">
            <in_mappings by_interface="true">
                <mapping src="bgdate" dest="bgdate" by_interface="true" on_top="true"/>
                <mapping src="brchno" dest="brchno" by_interface="true" on_top="true"/>
                <mapping src="cgpyrv" dest="cgpyrv" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="custac" by_interface="true" on_top="true"/>
                <mapping src="custno" dest="custno" by_interface="true" on_top="true"/>
                <mapping src="endate" dest="endate" by_interface="true" on_top="true"/>
                <mapping src="#run.pageno" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="#run.pgsize" dest="pagesz" by_interface="true" on_top="true"/>
                <mapping src="recvfg" dest="recvfg" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="disChargeInfo" dest="disChargeInfo" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
