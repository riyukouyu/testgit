<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qracrl" longname="查询收费核算规则定义" kind="1" package="cn.sunline.ltts.busi.cgtran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.cgtran.trans.intf">
        <input packMode="true">
            <field id="chrgcd" type="PbBaseType.U_CHRGCD" required="false" multi="false" array="false" longname="费种代码"/>
            <field id="chrgna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="费种代码名称"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="pinfos" type="IoCgChrg.IoChrgAccRuleInfo" required="false" multi="true" array="false" longname="收费核算规则复合类型明细"/>
            <field id="pcount" type="long" required="false" multi="false" array="false" longname="收费核算规则定义表条数"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoCgChrgSvcType.IoSelChrgAccRule" id="SelChrgAccRule" longname="查询收费核算规则定义">
            <in_mappings by_interface="true">
                <mapping src="chrgcd" dest="chrgcd" by_interface="true" on_top="true"/>
                <mapping src="chrgna" dest="chrgna" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="pcount" dest="pcount" by_interface="true" on_top="true"/>
                <mapping src="pinfos" dest="pinfos" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
