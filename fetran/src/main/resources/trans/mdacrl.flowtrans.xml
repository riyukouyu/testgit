<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="mdacrl" longname="修改收费核算规则定义" kind="2" package="cn.sunline.ltts.busi.cgtran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.cgtran.trans.intf">
        <input packMode="true">
            <field id="chrgcd" type="PbBaseType.U_CHRGCD" required="false" multi="false" array="false" longname="费种代码"/>
            <field id="chrgsg" type="PbEnumType.E_YES___" required="false" multi="false" array="false" longname="是否允许集中收费标志"/>
            <field id="chrgpd" type="PbBaseType.U_WEEKPD" required="false" multi="false" array="false" longname="收费周期"/>
            <field id="bllwtp" type="PbEnumType.E_BLLWTP" required="false" multi="false" array="false" longname="余额不足处理方式"/>
            <field id="fvrmde" type="FeEnumType.E_PREWAY" required="false" multi="false" array="false" longname="优惠方式"/>
            <field id="pfrtim" type="PbEnumType.E_PFRTIM" required="false" multi="false" array="false" longname="优惠返还时间"/>
            <field id="debkpd" type="PbBaseType.U_WEEKPD" required="false" multi="false" array="false" longname="欠费扣费周期"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="BaseDict.Comm.brchno"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service mappingToProperty="true" serviceName="IoSrvPbBranch.IoGetCenterBranch" id="IoGetCenterBranch" longname="获取中心机构清算中心">
            <in_mappings by_interface="true">
                <mapping src="#run.tranbr" dest="brchno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="false">
                <mapping src="brchInfo.brchno" dest="brchno" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <service serviceName="IoCgChrgSvcType.IoUpdChrgAccRule" id="UpdChrgAccRule" longname="修改收费核算规则定义" desc="修改收费核算规则定义">
            <in_mappings by_interface="true">
                <mapping src="bllwtp" dest="bllwtp" by_interface="true" on_top="true"/>
                <mapping src="brchno" dest="brchno" by_interface="true" on_top="true"/>
                <mapping src="chrgcd" dest="chrgcd" by_interface="true" on_top="true"/>
                <mapping src="chrgpd" dest="chrgpd" by_interface="true" on_top="true"/>
                <mapping src="chrgsg" dest="chrgsg" by_interface="true" on_top="true"/>
                <mapping src="debkpd" dest="debkpd" by_interface="true" on_top="true"/>
                <mapping src="fvrmde" dest="fvrmde" by_interface="true" on_top="true"/>
                <mapping src="pfrtim" dest="pfrtim" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
</flowtran>
