<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrdist" longname="优惠费用查询" kind="1" package="cn.sunline.ltts.busi.cgtran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[]]></description>
    <interface package="cn.sunline.ltts.busi.cgtran.trans.intf">
        <input packMode="true">
            <field id="custac" type="PbBaseType.U_CUSTAC" required="true" multi="false" array="false" longname="客户账号"/>
            <field id="bgdate" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="起始日期"/>
            <field id="endate" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="截止日期"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="hcount" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="历史优惠总笔数"/>
            <field id="hdisam" type="PbBaseType.U_TRANAM" required="false" multi="false" array="false" longname="历史优惠总金额"/>
            <field id="distamlist" type="IoCgChrg.IoCgDisamtInfo" required="false" multi="true" array="false" longname="费用优惠明细"/>
        </output>
        <property packMode="true">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页码"/>
            <field id="pagesize" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页容量"/>
            <field id="counts" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="总笔数"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow id="" test="" longname="" desc="">
        <method method="doTransBefore" id="doTransBefore" test="" longname="列表查询前处理" desc=""/>
        <service serviceName="IoCgChrgSvcType.IoSelDistam" id="SelDistam" test="" longname="查询费用优惠明细" desc="">
            <in_mappings by_interface="true">
                <mapping src="bgdate" dest="bgdate" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="custac" by_interface="true" on_top="true"/>
                <mapping src="endate" dest="endate" by_interface="true" on_top="true"/>
                <mapping src="pageno" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="pagesize" dest="pagesize" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="disamtinfo" dest="distamlist" by_interface="true" on_top="true"/>
                <mapping src="hcount" dest="hcount" by_interface="true" on_top="true"/>
                <mapping src="hdisam" dest="hdisam" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
