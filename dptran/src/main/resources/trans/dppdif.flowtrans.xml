<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="dppdif" longname="根据产品编号获取定存产品信息" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[根据产品编号获取产品信息
可以是定期的产品编号，也可以是活期的产品编号]]></description>
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="false">
            <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编号" ref="DpDict.Prod.prodcd"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="pdinfo" type="DpProdSvc.DppbDetailInfo" required="false" multi="false" array="false" longname="产品信息输出"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="DpProdSvcType.QryDpByProdcd" id="dppdif" longname="获取产品信息" desc="获取产品信息">
            <in_mappings by_interface="true">
                <mapping src="prodcd" dest="prodcd" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="pdInfo.deptif" dest="pdinfo.deptif" by_interface="true" on_top="true"/>
                <mapping src="pdInfo.maxiam" dest="pdinfo.maxiam" by_interface="true" on_top="true"/>
                <mapping src="pdInfo.miniam" dest="pdinfo.miniam" by_interface="true" on_top="true"/>
                <mapping src="pdInfo.presal" dest="pdinfo.presal" by_interface="true" on_top="true"/>
                <mapping src="pdInfo.remanm" dest="pdinfo.remanm" by_interface="true" on_top="true"/>
                <mapping src="pdInfo.svrule" dest="pdinfo.svrule" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
