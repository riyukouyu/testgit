<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="qutran" longname="交易记录查询" kind="1" package="cn.sunline.edsp.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.edsp.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="inmeid" type="BaseType.U_CHAR25" required="false" multi="false" array="false" longname="内部商户ID" ref="PbDict.JFD.inmeid"/>
            <field id="flowid" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="流水号"/>
            <field id="pagenum" type="SPBaseType.U_PAGENM" required="false" multi="false" array="false" longname="当前页数"/>
            <field id="pagesize" type="SPBaseType.U_PAGELT" required="false" multi="false" array="false" longname="每页记录数"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="tranList" type="DpTransfer.DpTrans" required="false" multi="true" array="false" longname="交易记录"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <method method="queryTrans" longname="查询交易记录"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
