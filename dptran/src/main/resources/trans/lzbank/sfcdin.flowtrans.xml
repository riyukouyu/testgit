    <flow>
        <method method="DealTransBefore" id="DealTransBefore" longname="转账前处理"/>
        <block id="block_1" test="issucc == #dict.BaseEnumType.E_YES___().NO" longname="判断执行标志">
            <service mappingToProperty="true" serviceName="IoInAccount.IoInAcdr" id="IoInAcdr" longname="内部户借方记账服务" desc="借：304103401网络柜面核心内往来">
                <in_mappings by_interface="true">
                    <mapping src="acbrch" dest="input.acbrch" by_interface="true" on_top="true"/>
                    <mapping src="busino" dest="input.busino" by_interface="true" on_top="true"/>
                    <mapping src="subsac" dest="input.subsac" desc="description" by_interface="true" on_top="true"/>
                    <mapping src="crcycd" dest="input.crcycd" by_interface="true" on_top="true"/>
                    <mapping src="dscrtx" dest="input.dscrtx" by_interface="true" on_top="true"/>
                    <mapping src="smrycd" dest="input.smrycd" by_interface="true" on_top="true"/>
                    <mapping src="incard" dest="input.toacct" by_interface="true" on_top="true"/>
                    <mapping src="inacna" dest="input.toacna" desc="description" by_interface="true" on_top="true"/>
                    <mapping src="tranam" dest="input.tranam" by_interface="true" on_top="true"/>
                    <mapping src="trantp" dest="input.trantp" by_interface="true" on_top="true"/>
                </in_mappings>
                <out_mappings by_interface="true">
                    <mapping src="output.acctna" dest="clacna" by_interface="true" on_top="true"/>
                    <mapping src="output.acctno" dest="clacno" by_interface="true" on_top="true"/>
                </out_mappings>
            </service>
            <case longname="转出方账户判断">
                <when id="darIsac" test="inglac!=#dict.BaseEnumType.E_YES___().YES" longname="电子账户" desc="电子账户">
                    <service serviceName="IoCaSevAccountLimit.IoAcSubQuota" id="IoAcSubQuota" longname="电子账户系统扣减客户额度">
                        <in_mappings by_interface="true">
                            <mapping src="chkqtn.accttp" dest="accttp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.aclmfg" dest="aclmfg" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.authtp" dest="authtp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.brchno" dest="brchno" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.custac" dest="custac" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.custid" dest="custid" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.custlv" dest="custlv" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.acctrt" dest="acctrt" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.limttp" dest="limttp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.pytltp" dest="pytltp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.rebktp" dest="rebktp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.risklv" dest="risklv" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.sbactp" dest="sbactp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.facesg" dest="facesg" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.custie" dest="custie" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.recpay" dest="recpay" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="servdt" dest="servdt" by_interface="true" on_top="true"/>
                            <mapping src="servsq" dest="servsq" by_interface="true" on_top="true"/>
                            <mapping src="servtp" dest="servtp" by_interface="true" on_top="true"/>
                            <mapping src="tranam" dest="tranam" by_interface="true" on_top="true"/>
                        </in_mappings>
                        <out_mappings by_interface="true"/>
                    </service>
                    <service serviceName="DpAcctSvcType.addPostAcctDp" id="addPostAcctDp" longname="电子账户存入记账处理">
                        <in_mappings by_interface="true">
                            <mapping src="inchld" dest="cplSaveAcctIn.acctno" desc="转入方子账号" by_interface="true" on_top="true"/>
                            <mapping src="incard" dest="cplSaveAcctIn.cardno" desc="转入方卡号" by_interface="true" on_top="true"/>
                            <mapping src="crcycd" dest="cplSaveAcctIn.crcycd" desc="币种" by_interface="true" on_top="true"/>
                            <mapping src="incsac" dest="cplSaveAcctIn.custac" desc="转入方电子账号ID" by_interface="true" on_top="true"/>
                            <mapping src="linkno" dest="cplSaveAcctIn.linkno" by_interface="true" on_top="true"/>
                            <mapping src="otacna" dest="cplSaveAcctIn.opacna" desc="转出方户名/内部户名" by_interface="true" on_top="true"/>
                            <mapping src="brchno" dest="cplSaveAcctIn.opbrch" by_interface="true" on_top="true"/>
                            <mapping src="stady1" dest="cplSaveAcctIn.remark" by_interface="true" on_top="true"/>
                            <mapping src="smrycd" dest="cplSaveAcctIn.smrycd" desc="摘要码" by_interface="true" on_top="true"/>
                            <mapping src="otcard" dest="cplSaveAcctIn.toacct" desc="转出方账号/内部户账号" by_interface="true" on_top="true"/>
                            <mapping src="tranam" dest="cplSaveAcctIn.tranam" by_interface="true" on_top="true"/>
                            <mapping src="macdrs" dest="cplSaveAcctIn.macdrs" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="imeino" dest="cplSaveAcctIn.imeino" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="udidno" dest="cplSaveAcctIn.udidno" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="teleno" dest="cplSaveAcctIn.teleno" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="trands" dest="cplSaveAcctIn.trands" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="servtp" dest="cplSaveAcctIn.servtp" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="prcscd" dest="cplSaveAcctIn.intrcd" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="mntrsq" dest="cplSaveAcctIn.transq" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="issucc" dest="cplSaveAcctIn.issucc" desc="description" by_interface="true" on_top="true"/>
                        </in_mappings>
                        <out_mappings by_interface="true"/>
                    </service>
                    <method method="DealAcctStatAndSett" id="DealAcctStatAndSett" longname="修改电子账户状态和休眠转活结息"/>
                    <method method="sendAppTraninInfoMsg" longname="充值推送APP消息"/>
                </when>
                <when id="darIsin" test="inglac==#dict.BaseEnumType.E_YES___().YES" longname="内部户" desc="内部户">
                    <service serviceName="IoInAccount.IoInAccr" id="ioInAccr" longname="内部户贷方记账" desc="内部户贷方记账">
                        <in_mappings by_interface="true">
                            <mapping src="incard" dest="input.acctno" by_interface="true" on_top="true"/>
                            <mapping src="crcycd" dest="input.crcycd" by_interface="true" on_top="true"/>
                            <mapping src="acbrch" dest="input.acbrch" by_interface="true" on_top="true"/>
                            <mapping src="clacno" dest="input.toacct" by_interface="true" on_top="true"/>
                            <mapping src="clacna" dest="input.toacna" desc="description" by_interface="true" on_top="true"/>
                            <mapping src="trantp" dest="input.trantp" by_interface="true" on_top="true"/>
                            <mapping src="tranam" dest="input.tranam" by_interface="true" on_top="true"/>
                            <mapping src="smrycd" dest="input.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
                            <mapping src="dscrtx" dest="input.dscrtx" by_interface="true" on_top="true"/>
                        </in_mappings>
                        <out_mappings by_interface="true"/>
                    </service>
                </when>
            </case>
            <service serviceName="IoSaveIoTransBill.SaveIoBill" id="SaveIoBill" longname="登记出入金登记簿">
                <in_mappings by_interface="true">
                    <mapping src="inchld" dest="acctno" by_interface="true" on_top="true"/>
                    <mapping src="brchno" dest="brchno" by_interface="true" on_top="true"/>
                    <mapping src="busino" dest="busino" by_interface="true" on_top="true"/>
                    <mapping src="busisq" dest="busisq" by_interface="true" on_top="true"/>
                    <mapping src="capitp" dest="capitp" by_interface="true" on_top="true"/>
                    <mapping src="incard" dest="cardno" by_interface="true" on_top="true"/>
                    <mapping src="chckdt" dest="chckdt" by_interface="true" on_top="true"/>
                    <mapping src="crcycd" dest="crcycd" by_interface="true" on_top="true"/>
                    <mapping src="incsac" dest="cuacno" by_interface="true" on_top="true"/>
                    <mapping src="fromtp" dest="fromtp" by_interface="true" on_top="true"/>
                    <mapping src="frondt" dest="frondt" by_interface="true" on_top="true"/>
                    <mapping src="fronsq" dest="fronsq" by_interface="true" on_top="true"/>
                    <mapping src="ioflag" dest="ioflag" by_interface="true" on_top="true"/>
                    <mapping src="keepdt" dest="keepdt" by_interface="true" on_top="true"/>
                    <mapping src="linkno" dest="linkno" by_interface="true" on_top="true"/>
                    <mapping src="prcscd" dest="prcscd" by_interface="true" on_top="true"/>
                    <mapping src="linkno" dest="remark" by_interface="true" on_top="true"/>
                    <mapping src="servtp" dest="servtp" by_interface="true" on_top="true"/>
                    <mapping src="tlcgam" dest="tlcgam" by_interface="true" on_top="true"/>
                    <mapping src="otcard" dest="toacct" by_interface="true" on_top="true"/>
                    <mapping src="otchld" dest="toacno" by_interface="true" on_top="true"/>
                    <mapping src="otbrch" dest="tobrch" by_interface="true" on_top="true"/>
                    <mapping src="otcsac" dest="toscac" by_interface="true" on_top="true"/>
                    <mapping src="tranam" dest="tranam" by_interface="true" on_top="true"/>
                    <mapping src="incorp" dest="incorp" by_interface="true" on_top="true"/>
                    <mapping src="otcorp" dest="otcorp" by_interface="true" on_top="true"/>
                </in_mappings>
                <out_mappings by_interface="true"/>
            </service>
        </block>
        <method method="DealTransAfter" id="DealTransAfter" longname="交易后处理"/>
