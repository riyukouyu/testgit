<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="sfcdot" longname="电子账户本行卡提现（互金）" kind="3" package="cn.sunline.ltts.busi.dptran.trans.lzbank" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.lzbank.intf">
        <input packMode="true">
            <field id="capitp" type="DpEnumType.E_CAPITP" required="false" multi="false" array="false" longname="资金交易类型"/>
            <field id="otcard" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转出方卡号/账号"/>
            <field id="otcstp" type="BaseEnumType.E_ACSETP" required="false" multi="false" array="false" longname="转出方子账号类型"/>
            <field id="otacno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转出方子账号"/>
            <field id="otacna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="转出方户名"/>
            <field id="otbrch" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="转出方账号所属机构"/>
            <field id="smrycd" type="BaseType.U_SMRYCD" required="false" multi="false" array="false" longname="摘要代码" ref="BaseDict.Comm.smrycd"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
            <field id="csextg" type="BaseEnumType.E_CSEXTG" required="false" multi="false" array="false" longname="钞汇标志"/>
            <field id="tranam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="交易金额"/>
            <field id="inisac" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="转入方是否电子账号"/>
            <field id="incard" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转入方卡号/账号"/>
            <field id="incstp" type="BaseEnumType.E_ACSETP" required="false" multi="false" array="false" longname="转入方子账号类型"/>
            <field id="inacno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转入方子账号"/>
            <field id="inacna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="转入方户名"/>
            <field id="inbrch" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="转入方账号所属机构"/>
            <field id="chckdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="对账日期"/>
            <field id="keepdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="清算日期"/>
            <field id="stady1" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="备用字段1"/>
            <field id="stady2" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="备用字段2"/>
            <field id="tlcgam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="收费总金额"/>
            <field id="chkpwd" type="DpAcctType.ChkPwdIN" required="false" multi="false" array="false" longname="密码验证参数"/>
            <field id="chkqtn" type="DpAcctType.ChkQtIN" required="false" multi="false" array="false" longname="额度中心参数"/>
            <field id="chrgpm" type="IoChrgComplexType.IoCgCalCenterReturn" required="false" multi="true" array="false" longname="公共收费参数"/>
            <field id="fromtp" type="DpEnumType.E_FROMTP" required="false" multi="false" array="false" longname="出入金类型"/>
            <field id="fronsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="支付前置流水"/>
            <field id="frondt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="支付前置日期"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水"/>
            <field id="mntrdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="主交易日期"/>
            <field id="mntrtm" type="BaseType.U_TRTMCH" required="false" multi="false" array="false" longname="主交易时间"/>
        </output>
        <property packMode="true">
            <field id="chgflg" type="PbEnumType.E_CHGFLG" required="false" multi="false" array="false" longname="记账标志"/>
            <field id="ischrg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否收费"/>
            <field id="otcsac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转出方电子账号ID"/>
            <field id="otchld" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转出方子账号"/>
            <field id="otcorp" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="转出方法人代码"/>
            <field id="incorp" type="BaseType.U_CORPNO" required="false" multi="false" array="false" longname="转入方法人代码"/>
            <field id="incsac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转入方电子账号ID"/>
            <field id="inchld" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转入方子账号"/>
            <field id="busisq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="业务跟踪编号"/>
            <field id="servsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="交易流水"/>
            <field id="servdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期"/>
            <field id="prcscd" type="BaseType.U_PRCSCD" required="false" multi="false" array="false" longname="交易码"/>
            <field id="ioflag" type="DpEnumType.E_IOFLAG" required="false" multi="false" array="false" longname="出入金标志"/>
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="电子账户归属机构"/>
            <field id="frosq1" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="渠道流水"/>
            <field id="frodt1" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="渠道日期"/>
            <field id="clactp" type="BaseEnumType.E_CLACTP" required="false" multi="false" array="false" longname="清算账户类型"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="负债账号"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
            <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="客户账号"/>
            <field id="toacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="对方账号"/>
            <field id="opacna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="对方户名"/>
            <field id="linkno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="连笔标志"/>
            <field id="smryds" type="BaseType.U_SMRYDS" required="false" multi="false" array="false" longname="摘要描述"/>
            <field id="servtp" type="MsType.U_CHANNEL" required="false" multi="false" array="false" longname="交易渠道"/>
            <field id="intrcd" type="BaseType.U_LTTSCD" required="false" multi="false" array="false" longname="内部交易码"/>
            <field id="issucc" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="验证流水是否已存在"/>
            <field id="acnoin" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="内部户帐号"/>
            <field id="acbcin" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="账户机构"/>
            <field id="toacin" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="对方账号"/>
            <field id="tonain" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="对方户名"/>
            <field id="busino" type="BaseType.U_BUSINO" required="false" multi="false" array="false" longname="业务代码"/>
            <field id="subsac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="子户号"/>
            <field id="acctna" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="内部户帐号名称"/>
            <field id="dscrtx" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="描述"/>
            <field id="clacno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="清算内部账号"/>
            <field id="clacna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="清算内部账号名"/>
            <field id="inisin" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="转入方账户是否内部户"/>
            <field id="toisin" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="转出方账户是否内部户"/>
        </property>
        <printer packMode="true"/>
    </interface>
    <flow>
        <method method="dealBefore" id="dealBefore" longname="交易前处理"/>
        <block id="block1" test="issucc == #dict.BaseEnumType.E_YES___().NO" longname="非重复流水">
            <case longname="转出方账户判断">
                <when id="darIsac" test="toisin==#dict.BaseEnumType.E_YES___().NO" longname="电子账户" desc="电子账户">
                    <service serviceName="IoCaSevAccountLimit.IoAcSubQuota" id="IoAcSubQuota" longname="电子账户系统扣减账户额度">
                        <in_mappings by_interface="true">
                            <mapping src="chkqtn.acctrt" dest="acctrt" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.accttp" dest="accttp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.aclmfg" dest="aclmfg" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.authtp" dest="authtp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.custid" dest="custid" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.custie" dest="custie" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.custlv" dest="custlv" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.recpay" dest="dcflag" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.facesg" dest="facesg" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.limttp" dest="limttp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.pytltp" dest="pytltp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.rebktp" dest="rebktp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.recpay" dest="recpay" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.risklv" dest="risklv" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.sbactp" dest="sbactp" by_interface="true" on_top="true"/>
                            <mapping src="servdt" dest="servdt" by_interface="true" on_top="true"/>
                            <mapping src="servsq" dest="servsq" by_interface="true" on_top="true"/>
                            <mapping src="servtp" dest="servtp" by_interface="true" on_top="true"/>
                            <mapping src="chkqtn.servsq" dest="strksq" by_interface="true" on_top="true"/>
                            <mapping src="tranam" dest="tranam" by_interface="true" on_top="true"/>
                        </in_mappings>
                        <out_mappings by_interface="true"/>
                    </service>
                    <service serviceName="DpAcctSvcType.addDrawAcctDp" id="addDrawAcctDp" longname="电子账户借方记账">
                        <in_mappings by_interface="true">
                            <mapping src="acctno" dest="cplDrawAcctIn.acctno" desc="负债账号" by_interface="true" on_top="true"/>
                            <mapping src="cardno" dest="cplDrawAcctIn.cardno" desc="卡号" by_interface="true" on_top="true"/>
                            <mapping src="crcycd" dest="cplDrawAcctIn.crcycd" desc="币种" by_interface="true" on_top="true"/>
                            <mapping src="custac" dest="cplDrawAcctIn.custac" desc="电子账号" by_interface="true" on_top="true"/>
                            <mapping src="intrcd" dest="cplDrawAcctIn.intrcd" desc="内部交易码" by_interface="true" on_top="true"/>
                            <mapping src="issucc" dest="cplDrawAcctIn.issucc" desc="流水重复标志" by_interface="true" on_top="true"/>
                            <mapping src="linkno" dest="cplDrawAcctIn.linkno" desc="连笔号" by_interface="true" on_top="true"/>
                            <mapping src="opacna" dest="cplDrawAcctIn.opacna" desc="对方户名" by_interface="true" on_top="true"/>
                            <mapping src="servtp" dest="cplDrawAcctIn.servtp" desc="交易渠道" by_interface="true" on_top="true"/>
                            <mapping src="smrycd" dest="cplDrawAcctIn.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
                            <mapping src="smryds" dest="cplDrawAcctIn.smryds" desc="摘要描述" by_interface="true" on_top="true"/>
                            <mapping src="toacct" dest="cplDrawAcctIn.toacct" desc="对方账号" by_interface="true" on_top="true"/>
                            <mapping src="tranam" dest="cplDrawAcctIn.tranam" desc="交易金额" by_interface="true" on_top="true"/>
                        </in_mappings>
                        <out_mappings by_interface="true"/>
                    </service>
                </when>
                <when id="darIsin" test="toisin==#dict.BaseEnumType.E_YES___().YES" longname="内部户" desc="内部户">
                    <service serviceName="IoInAccount.IoInAcdr" id="ioinacdr" longname="内部户借方记账" desc="内部户借方记账">
                        <in_mappings by_interface="true">
                            <mapping src="otcard" dest="input.acctno" by_interface="true" on_top="true"/>
                            <mapping src="crcycd" dest="input.crcycd" by_interface="true" on_top="true"/>
                            <mapping src="incard" dest="input.toacct" by_interface="true" on_top="true"/>
                            <mapping src="inacna" dest="input.toacna" by_interface="true" on_top="true"/>
                            <mapping src="inacno" dest="input.tosbac" by_interface="true" on_top="true"/>
                            <mapping src="tranam" dest="input.tranam" by_interface="true" on_top="true"/>
                            <mapping src="otacna" dest="input.acctna" by_interface="true" on_top="true"/>
                            <mapping src="dscrtx" dest="input.dscrtx" by_interface="true" on_top="true"/>
                        </in_mappings>
                        <out_mappings by_interface="true"/>
                    </service>
                </when>
            </case>
            <service mappingToProperty="true" serviceName="IoInAccount.IoInAccr" id="IoInAccr" longname="内部账户贷方记账">
                <in_mappings by_interface="true">
                    <mapping src="acbcin" dest="input.acbrch" desc="账户机构" by_interface="true" on_top="true"/>
                    <mapping src="acnoin" dest="input.acctno" desc="内部户帐号" by_interface="true" on_top="true"/>
                    <mapping src="busino" dest="input.busino" desc="业务代码" by_interface="true" on_top="true"/>
                    <mapping src="crcycd" dest="input.crcycd" desc="币种" by_interface="true" on_top="true"/>
                    <mapping src="smrycd" dest="input.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
                    <mapping src="subsac" dest="input.subsac" desc="子户号" by_interface="true" on_top="true"/>
                    <mapping src="toacin" dest="input.toacct" desc="对方账号" by_interface="true" on_top="true"/>
                    <mapping src="tonain" dest="input.toacna" desc="对方户名" by_interface="true" on_top="true"/>
                    <mapping src="tranam" dest="input.tranam" desc="交易金额" by_interface="true" on_top="true"/>
                </in_mappings>
                <out_mappings by_interface="true">
                    <mapping src="output.acctno" dest="clacno" by_interface="true" on_top="true"/>
                    <mapping src="output.acctna" dest="clacna" by_interface="true" on_top="true"/>
                </out_mappings>
            </service>
            <service serviceName="IoSaveIoTransBill.SaveIoBill" id="SaveIoBill" longname="登记出入金登记簿">
                <in_mappings by_interface="true">
                    <mapping src="inchld" dest="acctno" by_interface="true" on_top="true"/>
                    <mapping src="brchno" dest="brchno" by_interface="true" on_top="true"/>
                    <mapping src="busino" dest="busino" by_interface="true" on_top="true"/>
                    <mapping src="busisq" dest="busisq" by_interface="true" on_top="true"/>
                    <mapping src="capitp" dest="capitp" by_interface="true" on_top="true"/>
                    <mapping src="incard" dest="cardno" by_interface="true" on_top="true"/>
                    <mapping src="chckdt" dest="chckdt" by_interface="true" on_top="true"/>
                    <mapping src="crcycd" dest="crcycd" by_interface="true" on_top="true"/>
                    <mapping src="incsac" dest="cuacno" by_interface="true" on_top="true"/>
                    <mapping src="fromtp" dest="fromtp" by_interface="true" on_top="true"/>
                    <mapping src="frodt1" dest="frondt" by_interface="true" on_top="true"/>
                    <mapping src="frosq1" dest="fronsq" by_interface="true" on_top="true"/>
                    <mapping src="ioflag" dest="ioflag" by_interface="true" on_top="true"/>
                    <mapping src="keepdt" dest="keepdt" by_interface="true" on_top="true"/>
                    <mapping src="linkno" dest="linkno" by_interface="true" on_top="true"/>
                    <mapping src="prcscd" dest="prcscd" by_interface="true" on_top="true"/>
                    <mapping src="linkno" dest="remark" by_interface="true" on_top="true"/>
                    <mapping src="servtp" dest="servtp" by_interface="true" on_top="true"/>
                    <mapping src="tlcgam" dest="tlcgam" by_interface="true" on_top="true"/>
                    <mapping src="otcard" dest="toacct" by_interface="true" on_top="true"/>
                    <mapping src="otchld" dest="toacno" by_interface="true" on_top="true"/>
                    <mapping src="otbrch" dest="tobrch" by_interface="true" on_top="true"/>
                    <mapping src="otcsac" dest="toscac" by_interface="true" on_top="true"/>
                    <mapping src="tranam" dest="tranam" by_interface="true" on_top="true"/>
                    <mapping src="incorp" dest="incorp" by_interface="true" on_top="true"/>
                    <mapping src="otcorp" dest="otcorp" by_interface="true" on_top="true"/>
                </in_mappings>
                <out_mappings by_interface="true"/>
            </service>
        </block>
        <method method="sendMessage" id="sendMessage" longname="短信通知登记"/>
        <method method="dealAfter" id="dealAfter" longname="交易后处理"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
