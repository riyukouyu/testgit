<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qryfro" longname="根据冻结编号查询冻结信息" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="frozno" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="冻结编号" ref="DpDict.Froz.frozno"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="frozInfo" type="IoDpFrozComplexType.IofrozInfo" required="false" multi="false" array="false" longname="冻结信息"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoDpFrozSvcType.IoDpQryFroz" id="IoDpQryFroz" longname="根据冻结编号查询当前生效冻结信息" desc="根据冻结编号查询当前生效冻结信息">
            <in_mappings by_interface="true">
                <mapping src="frozno" dest="frozno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="frozInfo.cardno" dest="frozInfo.cardno" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.freddt" dest="frozInfo.freddt" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.frexog" dest="frozInfo.frexog" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.frogna" dest="frozInfo.frogna" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.frozdt" dest="frozInfo.frozdt" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.frozno" dest="frozInfo.frozno" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.frozsq" dest="frozInfo.frozsq" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.isflag" dest="frozInfo.isflag" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.stopms" dest="frozInfo.stopms" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.sttmle" dest="frozInfo.sttmle" by_interface="true" on_top="true"/>
                <mapping src="frozInfo.tranam" dest="frozInfo.tranam" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
