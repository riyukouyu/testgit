<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="dpfroz" longname="司法冻结" kind="2" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[暂时只支持指电子帐号的冻结
其他待完善]]></description>
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="frozwy" type="DpEnumType.E_FROZWY" required="false" multi="false" array="false" longname="冻解方式" ref="DpDict.Froz.frozwy"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号"/>
            <field id="acctid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="子账户开户标识" ref="DpDict.Acct.acctid"/>
            <field id="custna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="客户名称" ref="DpDict.Acct.custna"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="DpDict.Acct.crcycd"/>
            <field id="csextg" type="BaseEnumType.E_CSEXTG" required="false" multi="false" array="false" longname="账户钞汇标志" ref="DpDict.Acct.csextg"/>
            <field id="frctno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="冻结通知书编号" ref="DpDict.Froz.frctno"/>
            <field id="frozam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="冻结金额" ref="DpDict.Froz.frozam"/>
            <field id="frozdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="冻结日期" ref="DpDict.Froz.frozdt"/>
            <field id="freddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="冻结终止日期" ref="DpDict.Froz.freddt"/>
            <field id="frexog" type="BaseEnumType.E_LAWCOP" required="false" multi="false" array="false" longname="执法部门" ref="DpDict.Froz.frexog"/>
            <field id="frogna" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="冻结执法部门名称" ref="DpDict.Froz.frogna"/>
            <field id="frna01" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="冻结执法人员1姓名" ref="DpDict.Froz.frna01"/>
            <field id="idtp01" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="执法人员1证件种类"/>
            <field id="idno01" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="执法人员1证件号码" ref="DpDict.Froz.idno01"/>
            <field id="frna02" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="冻结执法人员2姓名" ref="DpDict.Froz.frna02"/>
            <field id="idtp02" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="执法人员2证件种类"/>
            <field id="idno02" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="执法人员2证件号码" ref="DpDict.Froz.idno02"/>
            <field id="frreas" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="冻结原因" ref="DpDict.Froz.frreas"/>
            <field id="remark" type="BaseType.U_SHORTS" required="false" multi="false" array="false" longname="备注" ref="DpDict.Froz.remark"/>
            <field id="frlmtp" type="DpEnumType.E_FRLMTP" required="false" multi="false" array="false" longname="限制类型" ref="DpDict.Froz.frlmtp"/>
            <field id="frozcd" type="BaseType.U_CHAR08" required="false" multi="false" array="false" longname="冻结分类码" ref="DpDict.Froz.frozcd"/>
            <field id="frozlv" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="冻结级别" ref="DpDict.Froz.frozlv"/>
            <field id="frozno" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="冻结编号" ref="DpDict.Froz.frozno"/>
            <field id="frozow" type="DpEnumType.E_FROZOW" required="false" multi="false" array="false" longname="冻结主体类型" ref="DpDict.Froz.frozow"/>
            <field id="froztp" type="DpEnumType.E_FROZTP" required="false" multi="false" array="false" longname="冻结业务类型" ref="DpDict.Froz.froztp"/>
            <field id="acctList" type="DpFrozType.acctInfo" required="false" multi="true" array="false" longname="冻结明细"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号" ref="BaseDict.Comm.cardno"/>
            <field id="frozno" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="冻结编号" ref="BaseDict.Comm.frozno"/>
            <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="BaseDict.Comm.trandt"/>
            <field id="transq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="交易流水" ref="BaseDict.Comm.transq"/>
        </output>
        <property packMode="true">
            <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="DpDict.Acct.custac"/>
            <field id="acctno2" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="子账号"/>
            <field id="cardno2" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="selAcdcByAcctid" id="selAcdcByAcctid" longname="根据子账户标识查询账户信息"/>
        <service mappingToProperty="false" serviceName="IoDpFrozSvcType.IoDpFrozForJF" id="IoDpFrozForJF" longname="即富冻结服务" desc="司法冻结">
            <in_mappings by_interface="true">
                <mapping src="acctList" dest="dpFrozInfo.acctList" by_interface="true" on_top="true"/>
                <mapping src="acctid" dest="dpFrozInfo.acctid" by_interface="true" on_top="true"/>
                <mapping src="acctno2" dest="dpFrozInfo.acctno" by_interface="true" on_top="true"/>
                <mapping src="cardno2" dest="dpFrozInfo.cardno" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="dpFrozInfo.crcycd" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="dpFrozInfo.custac" by_interface="true" on_top="true"/>
                <mapping src="frozdt" dest="dpFrozInfo.frozdt" by_interface="true" on_top="true"/>
                <mapping src="frozwy" dest="dpFrozInfo.frozwy" by_interface="true" on_top="true"/>
                <mapping src="frreas" dest="dpFrozInfo.frreas" by_interface="true" on_top="true"/>
                <mapping src="frozam" dest="dpFrozInfo.fztoam" desc="description" by_interface="true" on_top="true"/>
                <mapping src="frozlv" dest="dpFrozInfo.frozlv" desc="description" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="dpFrozOutInfo.cardno" dest="cardno" by_interface="true" on_top="true"/>
                <mapping src="dpFrozOutInfo.frozno" dest="frozno" by_interface="true" on_top="true"/>
                <mapping src="dpFrozOutInfo.trandt" dest="trandt" by_interface="true" on_top="true"/>
                <mapping src="dpFrozOutInfo.transq" dest="transq" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
