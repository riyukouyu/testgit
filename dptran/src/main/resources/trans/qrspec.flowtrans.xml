<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrspec" longname="查询特殊业务信息交易" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[查询特殊业务信息交易]]></description>
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="spectp" type="DpEnumType.E_SPECTP" required="false" multi="false" array="false" longname="特殊业务类型" ref="DpDict.Froz.spectp"/>
            <field id="frexog" type="BaseEnumType.E_LAWCOP" required="false" multi="false" array="false" longname="部门类型" ref="DpDict.Froz.frexog"/>
            <field id="stopms" type="DpEnumType.E_STOPMS" required="false" multi="false" array="false" longname="止付措施" ref="DpDict.Froz.stopms"/>
            <field id="tranbr" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="交易机构" ref="DpDict.Froz.tranbr"/>
            <field id="specno" type="BaseType.U_CHAR16" required="false" multi="false" array="false" longname="特殊业务编号" ref="DpDict.Froz.specno"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="specst" type="DpEnumType.E_SPECST" required="false" multi="false" array="false" longname="特殊业务状态" ref="DpDict.Froz.specst"/>
            <field id="bgindt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始交易日期"/>
            <field id="endddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="终止交易日期"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="specInfoList" type="IoDpFrozComplexType.IoDpSpecInfo" required="false" multi="true" array="false" longname="特殊业务信息集合"/>
            <field id="counts" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="总笔数"/>
        </output>
        <property packMode="false">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
            <field id="pgsize" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="每页记录数"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoDpFrozSvcType.IoQueryDpSpec" id="queryDpSpec" longname="查询特殊业务信息交易" desc="查询特殊业务信息交易">
            <in_mappings by_interface="true">
                <mapping src="bgindt" dest="specInfoListIn.bgindt" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="specInfoListIn.cardno" by_interface="true" on_top="true"/>
                <mapping src="endddt" dest="specInfoListIn.endddt" by_interface="true" on_top="true"/>
                <mapping src="frexog" dest="specInfoListIn.frexog" by_interface="true" on_top="true"/>
                <mapping src="pageno" dest="specInfoListIn.pageno" by_interface="true" on_top="true"/>
                <mapping src="pgsize" dest="specInfoListIn.pgsize" by_interface="true" on_top="true"/>
                <mapping src="specno" dest="specInfoListIn.specno" by_interface="true" on_top="true"/>
                <mapping src="specst" dest="specInfoListIn.specst" by_interface="true" on_top="true"/>
                <mapping src="spectp" dest="specInfoListIn.spectp" by_interface="true" on_top="true"/>
                <mapping src="stopms" dest="specInfoListIn.stopms" by_interface="true" on_top="true"/>
                <mapping src="tranbr" dest="specInfoListIn.tranbr" by_interface="true" on_top="true"/>
                <mapping src="#run.pageno" dest="pageno" desc="页码" by_interface="true" on_top="true"/>
                <mapping src="#run.pgsize" dest="pgsize" desc="页容量" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="counts" dest="counts" by_interface="true" on_top="true"/>
                <mapping src="specInfoList" dest="specInfoList" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
