<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="dprepd" longname="预约购买产品管理" kind="2" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="operat" type="DpEnumType.E_OPERAT" required="false" multi="false" array="false" longname="操作类型"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="DpDict.Acct.crcycd"/>
            <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编号" ref="DpDict.Prod.prodcd"/>
            <field id="depttm" type="BaseEnumType.E_TERMCD" required="false" multi="false" array="false" longname="存期" ref="DpDict.Prod.depttm"/>
            <field id="restdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="首次预约日期"/>
            <field id="cycles" type="DpEnumType.E_CYCLES" required="false" multi="false" array="false" longname="预约周期类型"/>
            <field id="recycl" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="预约周期"/>
            <field id="rebype" type="DpEnumType.E_REBYPE" required="false" multi="false" array="false" longname="预约方式"/>
            <field id="quanmy" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="定额购买金额"/>
            <field id="holdmy" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="留存金额"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="reserveProductList" type="DpProdSvc.ReserveProductOut" required="false" multi="true" array="false" longname="预约购买产品输出集合"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <method method="dprepd" longname="输入字段为空检验" desc="输入字段为空检验"/>
        <service serviceName="DpProdSvcType.MagDpReserveProduct" id="MagDpReserveProduct" longname="预约购买产品管理">
            <in_mappings by_interface="true">
                <mapping src="operat" dest="operat" desc="description" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cardno" desc="description" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="crcycd" desc="description" by_interface="true" on_top="true"/>
                <mapping src="prodcd" dest="prodcd" desc="description" by_interface="true" on_top="true"/>
                <mapping src="depttm" dest="depttm" desc="description" by_interface="true" on_top="true"/>
                <mapping src="restdt" dest="restdt" desc="description" by_interface="true" on_top="true"/>
                <mapping src="cycles" dest="cycles" desc="description" by_interface="true" on_top="true"/>
                <mapping src="recycl" dest="recycl" desc="description" by_interface="true" on_top="true"/>
                <mapping src="rebype" dest="rebype" desc="description" by_interface="true" on_top="true"/>
                <mapping src="quanmy" dest="quanmy" desc="description" by_interface="true" on_top="true"/>
                <mapping src="holdmy" dest="holdmy" desc="description" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="reserveProductOut" dest="reserveProductList" desc="description" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
