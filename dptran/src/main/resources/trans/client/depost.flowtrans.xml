<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="depost" longname="电子账户钱包账户提现" kind="3" package="cn.sunline.ltts.busi.dptran.trans.client" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.client.intf">
        <input packMode="true">
            <field id="tranam" type="BaseType.U_TRANAM" required="true" multi="false" array="false" longname="交易金额" ref="DpDict.Acct.tranam"/>
            <field id="csextg" type="BaseEnumType.E_CSEXTG" required="true" multi="false" array="false" longname="钞汇标志"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="币种" ref="BaseDict.Comm.crcycd"/>
            <field id="cardno" type="BaseType.U_ACCTNO" required="true" multi="false" array="false" longname="电子账号"/>
            <field id="acctna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="账户名称" ref="DpDict.Acct.acctna"/>
            <field id="toacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="钱包账户账号" ref="PbDict.Cash.toacct"/>
            <field id="smrycd" type="BaseType.U_SMRYCD" required="false" multi="false" array="false" longname="摘要代码" ref="BaseDict.Comm.smrycd"/>
            <field id="chckdt" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="对账日期"/>
            <field id="keepdt" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="清算日期"/>
            <field id="stady1" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="备用字段1"/>
            <field id="stady2" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="备用字段2"/>
            <field id="chkpwd" type="DpAcctType.ChkPwdIN" required="false" multi="false" array="false" longname="密码验证参数"/>
            <field id="chkqtn" type="DpAcctType.ChkQtIN" required="false" multi="false" array="false" longname="额度中心参数"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水"/>
            <field id="mntrdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="主交易日期"/>
            <field id="mntrtm" type="BaseType.U_TRTMCH" required="false" multi="false" array="false" longname="主交易时间"/>
        </output>
        <property packMode="true">
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="账户所属机构" ref="DpDict.Acct.brchno"/>
            <field id="busino" type="BaseType.U_BUSINO" required="false" multi="false" array="false" longname="业务编码" ref="PbDict.Dcmt.busino"/>
            <field id="linkno" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="连笔交易号" ref="PbDict.Comm.linkno"/>
            <field id="auacfg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="存取标志" ref="DpDict.Acct.auacfg"/>
            <field id="custac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="电子账号ID"/>
            <field id="acseno" type="BaseType.U_ACSENO" required="false" multi="false" array="false" longname="子账户号"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="负债账号" ref="DpDict.Acct.acctno"/>
            <field id="waacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="钱包账户账号"/>
            <field id="waacna" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="户名"/>
            <field id="smrycd1" type="BaseType.U_SMRYCD" required="false" multi="false" array="false" longname="摘要码"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="DealTransBefore" id="DealTransBefore" longname="交易前处理"/>
        <service serviceName="IoCaOtherService.IoCaChkpassword" id="IoCaChkpassword" test="chkpwd.ispass == #dict.BaseEnumType.E_YES___().YES" longname="统一认证验证密码">
            <in_mappings by_interface="true"/>
            <out_mappings by_interface="true"/>
        </service>
        <service serviceName="DpAcctSvcType.addDrawAcctDp" id="addDrawAcctDp" longname="电子账户支取记账服务">
            <in_mappings by_interface="true">
                <mapping src="waacct" dest="cplDrawAcctIn.acctno" by_interface="true" on_top="true"/>
                <mapping src="acseno" dest="cplDrawAcctIn.acseno" by_interface="true" on_top="true"/>
                <mapping src="auacfg" dest="cplDrawAcctIn.auacfg" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cplDrawAcctIn.cardno" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="cplDrawAcctIn.crcycd" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="cplDrawAcctIn.custac" by_interface="true" on_top="true"/>
                <mapping src="linkno" dest="cplDrawAcctIn.linkno" by_interface="true" on_top="true"/>
                <mapping src="waacna" dest="cplDrawAcctIn.opacna" by_interface="true" on_top="true"/>
                <mapping src="acctno" dest="cplDrawAcctIn.toacct" by_interface="true" on_top="true"/>
                <mapping src="tranam" dest="cplDrawAcctIn.tranam" by_interface="true" on_top="true"/>
                <mapping src="smrycd" dest="cplDrawAcctIn.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <service serviceName="DpAcctSvcType.addPostAcctDp" id="addPostAcctDp" longname="电子账户存入记账处理">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="cplSaveAcctIn.acctno" by_interface="true" on_top="true"/>
                <mapping src="acseno" dest="cplSaveAcctIn.acseno" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cplSaveAcctIn.cardno" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="cplSaveAcctIn.crcycd" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="cplSaveAcctIn.custac" by_interface="true" on_top="true"/>
                <mapping src="linkno" dest="cplSaveAcctIn.linkno" by_interface="true" on_top="true"/>
                <mapping src="acctna" dest="cplSaveAcctIn.opacna" by_interface="true" on_top="true"/>
                <mapping src="brchno" dest="cplSaveAcctIn.opbrch" by_interface="true" on_top="true"/>
                <mapping src="stady1" dest="cplSaveAcctIn.remark" by_interface="true" on_top="true"/>
                <mapping src="waacct" dest="cplSaveAcctIn.toacct" by_interface="true" on_top="true"/>
                <mapping src="tranam" dest="cplSaveAcctIn.tranam" by_interface="true" on_top="true"/>
                <mapping src="smrycd1" dest="cplSaveAcctIn.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <method method="DealTransAfter" id="DealTransAfter" longname="交易后处理"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
