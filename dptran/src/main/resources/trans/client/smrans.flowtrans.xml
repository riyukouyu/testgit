<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="smrans" longname="定期智能存款支取赎回" kind="3" package="cn.sunline.ltts.busi.dptran.trans.client" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.client.intf">
        <input packMode="true">
            <field id="tranam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="交易金额"/>
            <field id="csextg" type="BaseEnumType.E_CSEXTG" required="false" multi="false" array="false" longname="钞汇标志"/>
            <field id="cardno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="存款子账号"/>
            <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品代码"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
            <field id="chkpwd" type="DpAcctType.ChkPwdIN" required="false" multi="false" array="false" longname="验密参数"/>
            <field id="chnlid" type="BaseEnumType.E_CHNLID" required="false" multi="false" array="false" longname="交易渠道"/>
            <field id="ordrid" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="订单号" ref="DpDict.Insu.ordrid"/>
            <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型" ref="DpDict.Acct.idtftp"/>
            <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码" ref="DpDict.Acct.idtfno"/>
            <field id="ordedt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易订单日期"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水"/>
            <field id="mntrdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="主交易日期"/>
            <field id="mntrtm" type="BaseType.U_TRTMCH" required="false" multi="false" array="false" longname="主交易时间"/>
            <field id="isclos" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否销户"/>
            <field id="intere" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="支取利息"/>
            <field id="drdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="支取日期"/>
            <field id="transt" type="yht.E_TRANST" required="false" multi="false" array="false" longname="订单状态"/>
            <field id="isover" type="yht.E_ISOVER" required="false" multi="false" array="false" longname="是否到期"/>
            <field id="actamt" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="实际支取金额"/>
        </output>
        <property packMode="true">
            <field id="toacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="结算子账号"/>
            <field id="opacna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="客户名称"/>
            <field id="acseno" type="BaseType.U_ACSENO" required="false" multi="false" array="false" longname="子账户号"/>
            <field id="linkno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="连笔号"/>
            <field id="auacfg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="存取标志"/>
            <field id="custac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="系统账号"/>
            <field id="isclos" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否销户"/>
            <field id="instam" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="支取利息"/>
            <field id="intxam" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="利息税"/>
            <field id="acctam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="入账金额"/>
            <field id="smrycd" type="BaseType.U_SMRYCD" required="false" multi="false" array="false" longname="摘要代码" ref="BaseDict.Comm.smrycd"/>
            <field id="opbrch" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="开户机构"/>
            <field id="pyafam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="追缴利息"/>
            <field id="remark" type="BaseType.U_MIDDDS" required="false" multi="false" array="false" longname="备注信息"/>
            <field id="drdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="支取日期"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="DealTransBefore" id="DealTransBefore" longname="交易前处理"/>
        <service serviceName="IoCaOtherService.IoCaChkpassword" id="IoCaChkpassword" test="chkpwd.ispass == #dict.BaseEnumType.E_YES___().YES" longname="统一认证验证密码">
            <in_mappings by_interface="true"/>
            <out_mappings by_interface="true"/>
        </service>
        <service mappingToProperty="true" serviceName="DpAcctSvcType.addDrawAcctDp" id="addDrawAcctDp" longname="支取记账处理" desc="定期智能存款子账号支取处理">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="cplDrawAcctIn.acctno" desc="存款子账号" by_interface="true" on_top="true"/>
                <mapping src="acseno" dest="cplDrawAcctIn.acseno" desc="子账户号" by_interface="true" on_top="true"/>
                <mapping src="auacfg" dest="cplDrawAcctIn.auacfg" desc="存取标志" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cplDrawAcctIn.cardno" desc="卡号" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="cplDrawAcctIn.crcycd" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="cplDrawAcctIn.custac" desc="电子账号" by_interface="true" on_top="true"/>
                <mapping src="linkno" dest="cplDrawAcctIn.linkno" by_interface="true" on_top="true"/>
                <mapping src="opacna" dest="cplDrawAcctIn.opacna" desc="户名" by_interface="true" on_top="true"/>
                <mapping src="toacct" dest="cplDrawAcctIn.toacct" desc="结算子账号" by_interface="true" on_top="true"/>
                <mapping src="tranam" dest="cplDrawAcctIn.tranam" desc="交易金额" by_interface="true" on_top="true"/>
                <mapping src="opbrch" dest="cplDrawAcctIn.opbrch" desc="description" by_interface="true" on_top="true"/>
                <mapping src="remark" dest="cplDrawAcctIn.remark" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="cplDrawAcctOut.instam" dest="instam" by_interface="true" on_top="true"/>
                <mapping src="cplDrawAcctOut.intxam" dest="intxam" desc="description" by_interface="true" on_top="true"/>
                <mapping src="cplDrawAcctOut.pyafam" dest="pyafam" desc="追缴利息" by_interface="true" on_top="true"/>
                <mapping src="cplDrawAcctOut.drdate" dest="drdate" desc="支取日期" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <method method="DealInstam" id="DealInstam" longname="获取支取利息"/>
        <service serviceName="DpAcctSvcType.addPostAcctDp" id="addPostAcctDp" longname="存入记账处理" desc="结算子账号存入记账处理">
            <in_mappings by_interface="true">
                <mapping src="toacct" dest="cplSaveAcctIn.acctno" desc="结算子账号" by_interface="true" on_top="true"/>
                <mapping src="acseno" dest="cplSaveAcctIn.acseno" desc="子账户号" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cplSaveAcctIn.cardno" desc="卡号" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="cplSaveAcctIn.crcycd" desc="币种" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="cplSaveAcctIn.custac" desc="电子账号" by_interface="true" on_top="true"/>
                <mapping src="linkno" dest="cplSaveAcctIn.linkno" desc="连笔号" by_interface="true" on_top="true"/>
                <mapping src="opacna" dest="cplSaveAcctIn.opacna" desc="户名" by_interface="true" on_top="true"/>
                <mapping src="acctno" dest="cplSaveAcctIn.toacct" desc="存款子账号" by_interface="true" on_top="true"/>
                <mapping src="acctam" dest="cplSaveAcctIn.tranam" by_interface="true" on_top="true"/>
                <mapping src="remark" dest="cplSaveAcctIn.remark" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <service serviceName="DpAcctSvcType.addDrawAcctDp" id="addDrawAcctDp" test="intxam &gt; @java.math.BigDecimal@ZERO" longname="利息税支取入账" desc="从客户账扣除利息税">
            <in_mappings by_interface="true">
                <mapping src="toacct" dest="cplDrawAcctIn.acctno" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cplDrawAcctIn.cardno" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="cplDrawAcctIn.crcycd" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="cplDrawAcctIn.custac" by_interface="true" on_top="true"/>
                <mapping src="intxam" dest="cplDrawAcctIn.tranam" by_interface="true" on_top="true"/>
                <mapping src="smrycd" dest="cplDrawAcctIn.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
                <mapping src="remark" dest="cplDrawAcctIn.remark" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <method method="drawRegister" longname="支取信息登记"/>
        <method method="DealTransAfter" id="DealTransAfter" longname="交易后处理"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
