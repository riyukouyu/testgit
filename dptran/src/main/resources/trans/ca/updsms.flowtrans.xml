<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="updsms" longname="更新客户短信设置状态" kind="2" package="cn.sunline.ltts.busi.dptran.trans.ca" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptrans.trans.ca.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="true" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
            <field id="isopen" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否开通短信服务"/>
        </input>
        <output packMode="true"/>
        <property packMode="true">
        	<field id="teleno" type="BaseType.U_TELENO" required="false" multi="false" array="false" longname="电话号码" ref="DpDict.Acct.teleno"/>
            <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.custac"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="beforeTran" id="beforeTran" longname="交易前检查"/>
        <service serviceName="IoCaSevSmsManager.SmsOpenClose" id="SmsOpenClose" longname="设置是否开通短信通知">
            <in_mappings by_interface="true">
                <mapping src="custac" dest="custac" by_interface="true" on_top="true"/>
                <mapping src="teleno" dest="teleno" by_interface="true" on_top="true"/>
                <mapping src="isopen" dest="isopen" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
