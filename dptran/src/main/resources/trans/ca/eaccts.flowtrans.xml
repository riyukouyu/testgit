            <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
            <field id="custna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="客户名称" ref="DpDict.Acct.custna"/>
            <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型" ref="DpDict.Acct.idtftp"/>
            <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码" ref="DpDict.Acct.idtfno"/>
            <field id="teleno" type="BaseType.U_TELENO" required="false" multi="false" array="false" longname="电话号码" ref="DpDict.Acct.teleno"/>
            <field id="startt" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="起始记录数" ref="PbDict.Comm.startt"/>
            <field id="record" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="每页记录数" ref="PbDict.Comm.record"/>
        </input>
        <output asParm="true" packMode="true">
