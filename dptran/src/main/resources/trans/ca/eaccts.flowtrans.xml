                <mapping src="custna" dest="ecctin.custna" by_interface="true" on_top="true"/>
                <mapping src="ecctno" dest="ecctin.ecctno" by_interface="true" on_top="true"/>
                <mapping src="idtfno" dest="ecctin.idtfno" by_interface="true" on_top="true"/>
                <mapping src="idtftp" dest="ecctin.idtftp" by_interface="true" on_top="true"/>
                <mapping src="record" dest="ecctin.record" by_interface="true" on_top="true"/>
                <mapping src="startt" dest="ecctin.startt" by_interface="true" on_top="true"/>
                <mapping src="teleno" dest="ecctin.teleno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="ecctou.eaccts" dest="ecctou.eaccts" by_interface="true" on_top="true"/>
                <mapping src="ecctou.recdsm" dest="ecctou.recdsm" by_interface="true" on_top="true"/>
