                <field id="recdsm" type="long" required="false" multi="false" array="false" longname="总记录数"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow id="" test="" longname="" desc="">
        <service mappingToProperty="false" serviceName="IoCaSrvGenEAccountInfo.QryEacctInfos" id="qryEacctInfos" test="" longname="电子账户信息列表查询" desc="电子账户信息列表查询">
