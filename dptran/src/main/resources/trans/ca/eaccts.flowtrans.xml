<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="eaccts" longname="电子账户信息列表查询" kind="account" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
