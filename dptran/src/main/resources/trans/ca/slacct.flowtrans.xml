<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="slacct" longname="查询电子账户所有信息" kind="1" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[查询电子账户所有状态信息]]></description>
    <interface package="cn.sunline.ltts.busi.catran.trans.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型" ref="DpDict.Acct.idtftp"/>
            <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码" ref="DpDict.Acct.idtfno"/>
            <field id="acalno" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="绑定手机号码"/>
            <field id="custid" type="BaseType.U_CUSTID" required="false" multi="false" array="false" longname="用户ID" ref="BaseDict.Comm.custid"/>
            <field id="servtp2" type="BaseEnumType.E_CHNLID" required="false" multi="false" array="false" longname="渠道"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="acctInfosList" type="CaCustInfo.acctAllInfos" required="false" multi="true" array="false" longname="电子账户全部信息输出"/>
        </output>
        <property packMode="true"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="prcSelAcctAllInfos" id="prcSelAcctAllInfos" longname="查询电子账户全部信息" desc="查询电子账户全部状态信息"/>
    </flow>
</flowtran>
