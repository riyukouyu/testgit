<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="upagnt" longname="修改代理单位信息" kind="1" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[修改代理单位信息]]></description>
    <interface package="cn.sunline.ltts.busi.catran.trans.intf">
        <input packMode="true">
            <field id="agntno" type="BaseType.U_CHAR20" required="false" multi="false" array="false" longname="代理单位编号"/>
