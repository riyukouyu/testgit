<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="cadebc" longname="解除电子账户绑定卡号" kind="2" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.ltts.busi.catran.trans.intf">
        <input packMode="true">
            <field id="acbdtp" type="BaseEnumType.E_ACCTFG" required="false" multi="false" array="false" longname="绑定账户标识" ref="DpDict.Acct.acbdtp"/>
            <field id="cdopac" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="结算卡账号" ref="DpDict.Acct.cdopac"/>
            <field id="mactid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="电子主账户开户标识" ref="DpDict.Acct.mactid"/>
            <field id="acctid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="子账户开户标识" ref="DpDict.Acct.acctid"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true">
            <field id="mantwy" type="CaEnumType.E_MANTWY" required="false" fixed="'3'" multi="false" array="false" longname="维护方式" ref="CaDict.Eacct.mantwy"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.cardno"/>
            <field id="acbdno" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="绑定账户账号" ref="DpDict.Acct.acbdno"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service mappingToProperty="true" serviceName="IoCaSevGenBindCard.IoCaUnbindCard" id="IoCaUnbindCard" longname="解除结算卡绑定关系">
            <in_mappings by_interface="false">
                <mapping src="acbdtp" dest="acbdtp" by_interface="true" on_top="true"/>
                <mapping src="acctid" dest="acctid" by_interface="true" on_top="true"/>
                <mapping src="cdopac" dest="cdopac" by_interface="true" on_top="true"/>
                <mapping src="mactid" dest="mactid" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="acbdno" dest="acbdno" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cardno" by_interface="true" on_top="true"/>
                <mapping src="mantwy" dest="mantwy" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <service serviceName="IoCaSevGenBindCard.IoCaCardAlterReg" id="caCardAlterReg" longname="绑定账户信息修改登记簿查询" desc="绑定账户信息修改登记簿查询">
            <in_mappings by_interface="true">
                <mapping src="cardno" dest="bdAcctIn.cardno" desc="description" by_interface="true" on_top="true"/>
                <mapping src="cdopac" dest="bdAcctIn.odopac" by_interface="true" on_top="true"/>
                <mapping src="acbdno" dest="bdAcctIn.acbdno" desc="description" by_interface="true" on_top="true"/>
                <mapping src="mantwy" dest="bdAcctIn.mantwy" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
