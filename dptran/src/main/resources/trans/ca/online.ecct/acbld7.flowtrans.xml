<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="acbld7" longname="电子账户7日历史余额查询" kind="account" package="cn.sunline.ltts.busi.catran.trans.online.ecct" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[]]></description>
    <interface package="cn.sunline.ltts.busi.catran.trans.online.ecct.intf">
        <input packMode="false">
            <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="DpDict.Acct.crcycd"/>
            <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="DpDict.Acct.trandt"/>
        </input>
        <output asParm="false" packMode="false">
            <fields id="blOut" scope="ServEacctType.EacctBlHtyDtlOut" required="false" multi="false" longname="电子账户七日余额明细" array="false">
                <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
                <field id="trandl" type="ServEacctType.EacctBlHtyDtl" required="false" multi="true" array="false" longname="交易明细"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow id="" test="" longname="" desc="">
        <service serviceName="ServEacctSvcType.QryEacctBlHstDtl7Day" id="qryEacctBlHstDtl7Day" test="" longname="30日历史余额明细查询" desc="将之前7日的历史余额改为查询30日历史余额">
            <in_mappings by_interface="true">
                <mapping src="crcycd" dest="crcycd" by_interface="true" on_top="true"/>
                <mapping src="ecctno" dest="ecctno" by_interface="true" on_top="true"/>
                <mapping src="trandt" dest="trandt" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="htyblOut.ecctno" dest="blOut.ecctno" by_interface="true" on_top="true"/>
                <mapping src="htyblOut.trandl" dest="blOut.trandl" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
