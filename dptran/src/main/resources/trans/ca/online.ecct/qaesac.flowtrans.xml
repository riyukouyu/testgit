<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="qaesac" longname="根据卡号查询所有子户标识" kind="1" package="cn.sunline.edsp.busi.dptran.trans.ca.online.ecct" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.edsp.busi.dptran.trans.ca.online.ecct.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号" ref="BaseDict.Comm.cardno"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="subList" type="DpAcctType.AcctSub" required="false" multi="true" array="false" longname="子户信息"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="KnaAcctService.IQuerySubAcctId" id="IQuerySubAcctId" longname="查询电子主账户所有子户标识" desc="根据主账户信息，查询它下面所有子户的信息及子户开户标识">
            <in_mappings by_interface="true">
                <mapping src="cardno" dest="cardno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="subList" dest="subList" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
