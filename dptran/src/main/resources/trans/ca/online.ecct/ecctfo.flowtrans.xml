<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="ecctfo" longname="电子账户查询交易" kind="account" package="cn.sunline.ltts.busi.catran.trans.online.ecct" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[电子账户查询交易]]></description>
    <interface package="cn.sunline.ltts.busi.catran.trans.online.ecct.intf">
        <input packMode="true">
            <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="DpDict.Acct.crcycd"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="eacctInfoOut" scope="ServEacctType.EacctInfoOut" required="false" multi="false" longname="电子账户信息输出" array="false">
                <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="DpDict.Acct.crcycd"/>
                <field id="opendt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="开户日期" ref="DpDict.Prod.opendt"/>
                <field id="acname" type="ApBaseType.U_ZHHUZWMC" required="false" multi="false" array="false" longname="账户名称" ref="CaDict.Eacct.acname"/>
                <field id="ecctst" type="BaseEnumType.E_ACCTST" required="false" multi="false" array="false" longname="电子账户状态"/>
                <field id="bkactp" type="BaseEnumType.E_BSCDTP" required="false" multi="false" array="false" longname="银行方账户类型"/>
                <field id="lstbal" type="ServEacctType.EacctInfoBal" required="false" multi="true" array="false" longname="余额列表"/>
                <field id="ecctbl" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="电子账户余额"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow id="" test="" longname="" desc="">
        <service mappingToProperty="false" serviceName="ServEacctSvcType.QryEacctInfo" id="qryEacctInfo" test="" longname="电子账户信息查询" desc="电子账户信息查询">
            <in_mappings by_interface="false">
                <mapping src="crcycd" dest="crcycd" by_interface="true" on_top="true"/>
                <mapping src="ecctno" dest="ecctno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="false">
                <mapping src="cplxEacctInfo.acname" dest="eacctInfoOut.acname" desc="" by_interface="true" on_top="true"/>
                <mapping src="cplxEacctInfo.bkactp" dest="eacctInfoOut.bkactp" desc="" by_interface="true" on_top="true"/>
                <mapping src="cplxEacctInfo.crcycd" dest="eacctInfoOut.crcycd" desc="" by_interface="true" on_top="true"/>
                <mapping src="cplxEacctInfo.ecctbl" dest="eacctInfoOut.ecctbl" desc="" by_interface="true" on_top="true"/>
                <mapping src="cplxEacctInfo.ecctno" dest="eacctInfoOut.ecctno" desc="" by_interface="true" on_top="true"/>
                <mapping src="cplxEacctInfo.ecctst" dest="eacctInfoOut.ecctst" desc="" by_interface="true" on_top="true"/>
                <mapping src="cplxEacctInfo.lstbal" dest="eacctInfoOut.lstbal" desc="" by_interface="true" on_top="true"/>
                <mapping src="cplxEacctInfo.opendt" dest="eacctInfoOut.opendt" desc="" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
