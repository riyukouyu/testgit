<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="qrsbat" longname="查询电子账户子户信息" kind="1" package="cn.sunline.edsp.busi.dptran.trans.ca.online.ecct" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[查询电子账户子户信息]]></description>
    <interface package="cn.sunline.edsp.busi.dptran.trans.ca.online.ecct.intf">
        <input packMode="true">
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="子账号" ref="DpDict.Acct.acctno"/>
            <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号" ref="DpDict.Acct.custno"/>
            <field id="acctna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="子账户名称" ref="DpDict.Acct.acctna"/>
            <field id="acctid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="子账户开户标识" ref="DpDict.Acct.acctid"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.cardno"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="subAcctInfoList" type="DpAcctType.AcctSbad" required="false" multi="true" array="false" longname="子户全部信息输出"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="KnaAcctService.IQuerySubAcctInfo" id="IQuerySubAcctInfo" longname="查询电子子账户信息列表">
            <in_mappings by_interface="true">
                <mapping src="acctid" dest="acctid" by_interface="true" on_top="true"/>
                <mapping src="acctna" dest="acctna" by_interface="true" on_top="true"/>
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cardno" desc="description" by_interface="true" on_top="true"/>
                <mapping src="custno" dest="custno" desc="description" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="subAcctInfoList" dest="subAcctInfoList" desc="description" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
