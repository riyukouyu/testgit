<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="ecctdl" longname="交易明细" kind="account" package="cn.sunline.ltts.busi.catran.trans.online.ecct" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[]]></description>
    <interface package="cn.sunline.ltts.busi.catran.trans.online.ecct.intf">
        <input packMode="true">
            <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="PbDict.Intr.crcycd"/>
            <field id="bgindt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="开始日期"/>
            <field id="eenddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="结束日期"/>
            <field id="pageno" type="long" required="false" multi="false" array="false" longname="页数"/>
            <field id="recdct" type="long" required="false" multi="false" array="false" longname="每页容量"/>
            <field id="eccttp" type="BaseEnumType.E_ECCTTP" required="false" multi="false" array="false" longname="账户类型"/>
            <field id="detlsq" type="long" required="false" multi="false" array="false" longname="顺序号"/>
            <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期" ref="DpDict.Acct.trandt"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="dtlOut" scope="ServEacctType.EacctTransDtlInfoOut" required="false" multi="false" longname="电子账户明细输出" array="false">
                <field id="ecctno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号" ref="CaDict.Eacct.ecctno"/>
                <field id="ecctlt" type="ServEacctType.EacctTransDtlInfo" required="false" multi="true" array="false" longname="明细信息"/>
                <field id="recdsm" type="int" required="false" multi="false" array="false" longname="总记录数"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow id="" test="" longname="" desc="">
        <service serviceName="ServEacctSvcType.QryEacctTransDtlInfo" id="qryEacctTransDtlInfo" test="" longname="电子账户交易明细查询" desc="明细查询">
            <in_mappings by_interface="false">
                <mapping src="bgindt" dest="transDtlInfoIn.bgindt" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="transDtlInfoIn.crcycd" by_interface="true" on_top="true"/>
                <mapping src="detlsq" dest="transDtlInfoIn.detlsq" by_interface="true" on_top="true"/>
                <mapping src="ecctno" dest="transDtlInfoIn.ecctno" by_interface="true" on_top="true"/>
                <mapping src="eccttp" dest="transDtlInfoIn.eccttp" by_interface="true" on_top="true"/>
                <mapping src="eenddt" dest="transDtlInfoIn.eenddt" by_interface="true" on_top="true"/>
                <mapping src="pageno" dest="transDtlInfoIn.pageno" by_interface="true" on_top="true"/>
                <mapping src="recdct" dest="transDtlInfoIn.recdct" by_interface="true" on_top="true"/>
                <mapping src="trandt" dest="transDtlInfoIn.trandt" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="transDtlInfoOut.ecctlt" dest="dtlOut.ecctlt" by_interface="true" on_top="true"/>
                <mapping src="transDtlInfoOut.ecctno" dest="dtlOut.ecctno" by_interface="true" on_top="true"/>
                <mapping src="transDtlInfoOut.recdsm" dest="dtlOut.recdsm" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
