                <mapping src="qracwy" dest="cplQracctIn.qracwy" desc="查询方式" by_interface="true" on_top="true"/>
                <mapping src="depttm" dest="cplQracctIn.depttm" desc="description" by_interface="true" on_top="true"/>
                <mapping src="astate" dest="cplQracctIn.astate" desc="description" by_interface="true" on_top="true"/>
                <mapping src="prodcd" dest="cplQracctIn.prodcd" desc="description" by_interface="true" on_top="true"/>
                <mapping src="acctList" dest="cplQracctIn.acctList" desc="账号列表" by_interface="true" on_top="true"/>
                <mapping src="proyht" dest="cplQracctIn.proyht" desc="description" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="cplDpacctInfos.cplQracctInfo" dest="dpacctInfoList" desc="存款类子账户信息" by_interface="true" on_top="true"/>
