<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrdpai" longname="查询电子账户存款类子账户信息(柜面)" kind="1" package="cn.sunline.ltts.busi.catran.trans.online.ecct" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.catran.trans.online.ecct.intf">
        <input packMode="true">
            <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型" ref="DpDict.Acct.idtftp"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
