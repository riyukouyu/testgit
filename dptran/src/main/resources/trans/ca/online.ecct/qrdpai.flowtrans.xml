            <field id="prodtp" type="CaEnumType.E_PRODTP" required="false" multi="false" array="false" longname="产品类型" ref="CaDict.Eacct.prodtp"/>
            <field id="acctst" type="CaEnumType.E_CUACST" required="false" multi="false" array="false" longname="客户账户状态"/>
            <field id="qracwy" type="BaseEnumType.E_QRACWY" required="false" multi="false" array="false" longname="查询方式"/>
            <field id="qractp" type="BaseEnumType.E_QRYCTP" required="false" multi="false" array="false" longname="查询类型"/>
            <field id="depttm" type="BaseEnumType.E_TERMCD" required="false" multi="false" array="false" longname="存期"/>
