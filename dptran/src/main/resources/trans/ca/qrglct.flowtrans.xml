<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrglct" longname="查询全功能账户数量" kind="1" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.catran.trans.intf">
        <input packMode="true">
            <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型"/>
            <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码"/>
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="账户所属机构" ref="DpDict.Acct.brchno"/>
            <field id="standy" type="BaseType.U_CHAR100" required="false" multi="false" array="false" longname="备用字段"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="counts" type="int" required="false" multi="false" array="false" longname="已开立账户总数"/>
            <field id="goblct" type="int" required="false" multi="false" array="false" longname="已开立全功能账户数量"/>
            <field id="finact" type="int" required="false" multi="false" array="false" longname="已开立理财户数量"/>
            <field id="waltct" type="int" required="false" multi="false" array="false" longname="已开立钱包账户数量"/>
            <field id="standy" type="BaseType.U_CHAR100" required="false" multi="false" array="false" longname="备用字段"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="ServEacctSvcType.QryGlcounts" id="qryGlcounts" longname="查询已开立一类户电子账户信息">
            <in_mappings by_interface="true">
                <mapping src="brchno" dest="brchno" by_interface="true" on_top="true"/>
                <mapping src="idtfno" dest="idtfno" by_interface="true" on_top="true"/>
                <mapping src="idtftp" dest="idtftp" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="counts" dest="counts" by_interface="true" on_top="true"/>
                <mapping src="finact" dest="finact" by_interface="true" on_top="true"/>
                <mapping src="globct" dest="goblct" by_interface="true" on_top="true"/>
                <mapping src="waltct" dest="waltct" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
