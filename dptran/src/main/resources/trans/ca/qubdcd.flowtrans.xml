<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="qubdcd" longname="客户结算卡列表查询" kind="1" package="cn.sunline.edsp.busi.dptran.trans.ca" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[查询电子账户绑定卡信息]]></description>
    <interface package="cn.sunline.edsp.busi.dptran.trans.ca.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.cardno"/>
            <field id="cdopna" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="结算卡姓名" ref="DpDict.Acct.cdopna"/>
            <field id="cdopac" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="结算卡账号" ref="DpDict.Acct.cdopac"/>
            <field id="tlphno" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="手机号码" ref="DpDict.Acct.tlphno"/>
            <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码" ref="DpDict.Acct.idtfno"/>
            <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型" ref="DpDict.Acct.idtftp"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="bindCardInfo" type="IoCaTypeGenBindCard.KnaCacdInfo" required="false" multi="true" array="false" longname="电子账户绑定卡信息"/>
        </output>
        <property packMode="true">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
            <field id="pagesize" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="每页记录数"/>
        </property>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service mappingToProperty="false" serviceName="IoCaSevQryTableInfo.qrybindCarInfoByCano" id="KnaCacdInfo" longname="客户结算卡列表查询">
            <in_mappings by_interface="true">
                <mapping src="cardno" dest="cardno" by_interface="true" on_top="true"/>
                <mapping src="cdopac" dest="cdopac" by_interface="true" on_top="true"/>
                <mapping src="cdopna" dest="cdopna" by_interface="true" on_top="true"/>
                <mapping src="idtfno" dest="idtfno" by_interface="true" on_top="true"/>
                <mapping src="idtftp" dest="idtftp" by_interface="true" on_top="true"/>
                <mapping src="tlphno" dest="tlphno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="bindCardInfo" dest="bindCardInfo" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
