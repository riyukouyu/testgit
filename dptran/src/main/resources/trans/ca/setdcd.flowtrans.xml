<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="setdcd" longname="设置默认卡" kind="2" package="cn.sunline.edsp.busi.dptran.trans.ca" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.edsp.busi.dptran.trans.ca.intf">
        <input packMode="true">
            <field id="acbdtp" type="BaseEnumType.E_ACCTFG" required="false" multi="false" array="false" longname="绑定账户标识" ref="DpDict.Acct.acbdtp"/>
            <field id="cdopac" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="结算卡账号" ref="DpDict.Acct.cdopac"/>
            <field id="mactid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="电子主账户开户标识" ref="DpDict.Acct.mactid"/>
            <field id="acctid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="子账户开户标识" ref="DpDict.Acct.acctid"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="IoCaSevGenBindCard.IoCaSetDefaultCard" id="IoCaSetDefaultCard" longname="设置默认卡">
            <in_mappings by_interface="true">
                <mapping src="acbdtp" dest="acbdtp" by_interface="true" on_top="true"/>
                <mapping src="acctid" dest="acctid" by_interface="true" on_top="true"/>
                <mapping src="cdopac" dest="cdopac" by_interface="true" on_top="true"/>
                <mapping src="mactid" dest="mactid" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
</flowtran>
