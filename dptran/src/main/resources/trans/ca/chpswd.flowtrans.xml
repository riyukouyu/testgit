<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="chpswd" longname="修改交易密码" kind="account" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.catran.trans.intf">
        <input packMode="false">
            <field id="custac" type="BaseType.U_CUACNO" required="true" multi="false" array="false" longname="电子账号"/>
            <field id="passwd" type="BaseType.U_TRANPW" required="true" multi="false" array="false" longname="交易密码"/>
            <field id="nwpswd" type="BaseType.U_TRANPW" required="true" multi="false" array="false" longname="新交易密码"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoSrvDpPasswd.UpdatePasswdComm" id="UpdatePasswdComm" longname="修改密码" desc="修改电子账户密码">
            <in_mappings by_interface="true">
                <mapping src="custac" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="passwd" dest="odpswd" by_interface="true" on_top="true"/>
                <mapping src="nwpswd" dest="passwd" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
    </flow>
</flowtran>
