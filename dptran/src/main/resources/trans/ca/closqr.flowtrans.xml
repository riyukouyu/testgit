<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="closqr" longname="查询销户登记薄" kind="1" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.catran.trans.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="orgnbr" type="CcBaseType.U_ORGNBR" required="false" multi="false" array="false" longname="机构号"/>
            <field id="bgindt" type="LnBaseType.U_DATE08" required="false" multi="false" array="false" longname="起始日期"/>
            <field id="matudt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="到期日期 "/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种" ref="CaDict.Eacct.crcycd"/>
            <field id="csextg" type="BaseEnumType.E_CSEXTG" required="false" multi="false" array="false" longname="钞汇标志" ref="CaDict.Eacct.csextg"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="closqrInfoList" type="IoCaTypGenEAccountInfo.IoCaQryInfoList" required="false" multi="true" array="false" longname="销户登记簿输出查询"/>
        </output>
        <property packMode="true">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
            <field id="pagesize" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页容量"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="prcQryInfoList" id="prcQryInfoList" longname="交易前处理" desc="销户登记薄查询交易前处理"/>
        <service serviceName="IoCaSevQryAccout.IoCaCloseQryInfoList" id="CaCloseQryInfoList" longname="销户查询">
            <in_mappings by_interface="true">
                <mapping src="bgindt" dest="bgindt" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="crcycd" by_interface="true" on_top="true"/>
                <mapping src="csextg" dest="csextg" by_interface="true" on_top="true"/>
                <mapping src="matudt" dest="matudt" by_interface="true" on_top="true"/>
                <mapping src="orgnbr" dest="orgnbr" by_interface="true" on_top="true"/>
                <mapping src="pageno" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="pagesize" dest="pagesize" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cardno" desc="description" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="closqrInfoList" dest="closqrInfoList" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
