<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="btoprt" longname="查询批量开户结果" kind="1" package="cn.sunline.ltts.busi.catran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[电子账户信息列表查询]]></description>
    <interface package="cn.sunline.ltts.busi.catran.trans.intf">
        <input packMode="true">
            <field id="btchno" type="BaseType.U_CHAR40" required="true" multi="false" array="false" longname="批量交易批次号" ref="BaseDict.Comm.btchno"/>
            <field id="openrt" type="CaEnumType.E_OPENRS" required="false" multi="false" array="false" longname="开户结果"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="btoprtInfoList" scope="ServEacctType.EacctBtoprtInfo" required="false" multi="true" longname="查询开户结果信息" array="false">
                <field id="cardno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号"/>
                <field id="custna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="绑定账户名称"/>
                <field id="accttp" type="BaseEnumType.E_ACCTTP" required="false" multi="false" array="false" longname="账户分类"/>
                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="归属机构"/>
                <field id="idtftp" type="BaseType.U_CHAR20" required="false" multi="false" array="false" longname="证件类型"/>
                <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码" ref="DpDict.Acct.idtfno"/>
                <field id="custid" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户编号" ref="CcDict.Lion.custid"/>
                <field id="agntna" type="BaseType.U_ANGTNA" required="false" multi="false" array="false" longname="代理单位名称"/>
                <field id="openrs" type="CaEnumType.E_OPENRT" required="false" multi="false" array="false" longname="开户结果"/>
                <field id="losern" type="BaseType.U_LOSERN" required="false" multi="false" array="false" longname="失败原因"/>
            </fields>
            <field id="count" type="long" required="false" multi="false" array="false" longname="总笔数"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service mappingToProperty="false" serviceName="IoCaSrvGenEAccountInfo.QryOpenrtResult" id="qryOpenrtResult" longname="查询批量开户结果" desc="查询批量开户结果">
            <in_mappings by_interface="true">
                <mapping src="btchno" dest="btchno" by_interface="true" on_top="true"/>
                <mapping src="openrt" dest="openrt" by_interface="true" on_top="true"/>
                <mapping src="#run.pageno" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="#run.pgsize" dest="pgsize" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="btoprtInfoList" dest="btoprtInfoList" by_interface="true" on_top="true"/>
                <mapping src="count" dest="count" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
