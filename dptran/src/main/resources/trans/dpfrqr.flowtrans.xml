<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="dpfrqr" longname="冻结查询" kind="query" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <description><![CDATA[1.按冻结编号查询。后面的过滤条件无效。（有冻结编号输入，则按此方法查询）
2.按客户账号查询。(无冻结编号也无子账号输入，则按此方法查询)
3.按客户账号+子账号查询。（无冻结编号输入，有子账号输入则按此方法查询）
默认查询返回15条记录。
暂时未实现分页]]></description>
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="frozno" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="冻结编号" ref="DpDict.Froz.frozno"/>
            <field id="acctid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="子账户开户标识" ref="DpDict.Acct.acctid"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
            <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.custac"/>
            <field id="subsac" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否子账户冻结"/>
            <field id="froztp" type="DpEnumType.E_FROZTP" required="false" multi="false" array="false" longname="冻结业务类型" ref="DpDict.Froz.froztp"/>
            <field id="frlmtp" type="DpEnumType.E_FRLMTP" required="false" multi="false" array="false" longname="限制类型" ref="DpDict.Froz.frlmtp"/>
            <field id="frozam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="冻结金额" ref="DpDict.Froz.frozam"/>
            <field id="frozst" type="DpEnumType.E_FROZST" required="false" multi="false" array="false" longname="冻结状态" ref="DpDict.Froz.frozst"/>
            <field id="bgdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="查询起始日期"/>
            <field id="eddate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="查询终止日期"/>
            <field id="bgrecd" type="long" required="false" default="0" multi="false" array="false" longname="查询起始笔数"/>
            <field id="counts" type="long" required="false" default="10" multi="false" array="false" longname="查询笔数" desc="不输入默认为10"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="frozif" scope="IoDpFrozComplexType.IoDpFrozIf" required="false" multi="true" longname="冻结明细信息" array="false">
                <field id="frozno" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="冻结编号" ref="DpDict.Froz.frozno"/>
                <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.custac"/>
                <field id="froztp" type="DpEnumType.E_FROZTP" required="false" multi="false" array="false" longname="冻结业务类型" ref="DpDict.Froz.froztp"/>
                <field id="frozcd" type="BaseType.U_CHAR08" required="false" multi="false" array="false" longname="冻结分类码" ref="DpDict.Froz.frozcd"/>
                <field id="frlmtp" type="DpEnumType.E_FRLMTP" required="false" multi="false" array="false" longname="限制类型" ref="DpDict.Froz.frlmtp"/>
                <field id="frozow" type="DpEnumType.E_FROZOW" required="false" multi="false" array="false" longname="冻结主体类型" ref="DpDict.Froz.frozow"/>
                <field id="frozlv" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="冻结级别" ref="DpDict.Froz.frozlv"/>
                <field id="frctno" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="冻结通知书编号" ref="DpDict.Froz.frctno"/>
                <field id="frcttp" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="冻结证明文书类别" ref="DpDict.Froz.frcttp"/>
                <field id="frreas" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="冻结原因" ref="DpDict.Froz.frreas"/>
                <field id="frexog" type="BaseEnumType.E_LAWCOP" required="false" multi="false" array="false" longname="执法部门" ref="DpDict.Froz.frexog"/>
                <field id="frogna" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="冻结执法部门名称" ref="DpDict.Froz.frogna"/>
                <field id="idtp01" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="执法人员1证件种类"/>
                <field id="idno01" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="执法人员1证件号码" ref="DpDict.Froz.idno01"/>
                <field id="frna01" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="冻结执法人员1姓名" ref="DpDict.Froz.frna01"/>
                <field id="idtp02" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="执法人员2证件种类"/>
                <field id="idno02" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="执法人员2证件号码" ref="DpDict.Froz.idno02"/>
                <field id="frna02" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="冻结执法人员2姓名" ref="DpDict.Froz.frna02"/>
                <field id="tranbr" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="交易机构"/>
                <field id="tranus" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="经办人"/>
                <field id="ckuser" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="复核人"/>
                <field id="pruser" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="审批人"/>
                <field id="frozdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="冻结日期" ref="DpDict.Froz.frozdt"/>
                <field id="froztm" type="BaseType.U_TRTMCH" required="false" multi="false" array="false" longname="冻结时间" ref="DpDict.Froz.froztm"/>
                <field id="frbgdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="冻结起始日期" ref="DpDict.Froz.frbgdt"/>
                <field id="freddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="冻结终止日期" ref="DpDict.Froz.freddt"/>
                <field id="frspfg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="轮候冻结标志" ref="DpDict.Froz.frspfg"/>
                <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水"/>
                <field id="ussqno" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="柜员流水号" ref="DpDict.Acct.ussqno"/>
                <field id="cptrcd" type="BaseType.U_PRCSCD" required="false" multi="false" array="false" longname="对账代码"/>
                <field id="lttscd" type="BaseType.U_LTTSCD" required="false" multi="false" array="false" longname="内部交易码"/>
                <field id="servtp" type="MsType.U_CHANNEL" required="false" multi="false" array="false" longname="交易渠道"/>
                <field id="frozsq" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="冻结序号" ref="DpDict.Froz.frozsq"/>
                <field id="frowid" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="冻结主体ID" ref="DpDict.Froz.frowid"/>
                <field id="frsbsq" type="BaseType.U_LONGIN" required="false" multi="false" array="false" longname="子冻结序号(同级别冻结序号)" ref="DpDict.Froz.frsbsq"/>
                <field id="frozam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="冻结金额" ref="DpDict.Froz.frozam"/>
                <field id="frozbl" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="冻结余额" ref="DpDict.Froz.frozbl"/>
                <field id="frozst" type="DpEnumType.E_FROZST" required="false" multi="false" array="false" longname="冻结状态" ref="DpDict.Froz.frozst"/>
            </fields>
        </output>
        <property packMode="true">
            <field id="custac2" type="BaseType.U_CUSTAC" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="子账号" ref="DpDict.Acct.acctno"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="selAcctByAcctid" id="selAcctByAcctid" longname="根据产品id查询账户信息"/>
        <service serviceName="IoDpFrozSvcType.IoDpFrozQuery" id="dpfrozqr" longname="冻结查询" desc="冻结查询">
            <in_mappings by_interface="true">
                <mapping src="bgdate" dest="cpliodpfrozqrin.bgdate" by_interface="true" on_top="true"/>
                <mapping src="bgrecd" dest="cpliodpfrozqrin.bgrecd" by_interface="true" on_top="true"/>
                <mapping src="acctno" dest="cpliodpfrozqrin.cardno" by_interface="true" on_top="true"/>
                <mapping src="counts" dest="cpliodpfrozqrin.counts" by_interface="true" on_top="true"/>
                <mapping src="custac2" dest="cpliodpfrozqrin.custac" by_interface="true" on_top="true"/>
                <mapping src="eddate" dest="cpliodpfrozqrin.eddate" by_interface="true" on_top="true"/>
                <mapping src="frlmtp" dest="cpliodpfrozqrin.frlmtp" by_interface="true" on_top="true"/>
                <mapping src="frozam" dest="cpliodpfrozqrin.frozam" by_interface="true" on_top="true"/>
                <mapping src="frozno" dest="cpliodpfrozqrin.frozno" by_interface="true" on_top="true"/>
                <mapping src="frozst" dest="cpliodpfrozqrin.frozst" by_interface="true" on_top="true"/>
                <mapping src="froztp" dest="cpliodpfrozqrin.froztp" by_interface="true" on_top="true"/>
                <mapping src="subsac" dest="cpliodpfrozqrin.subsac" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="cpliodpfrozqrot.lsfroz" dest="frozif" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
