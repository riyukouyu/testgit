<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrpecb" longname="查询智能储蓄每天预期收益交易" kind="query" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="false">
            <field id="custac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="bigndt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始日期"/>
            <field id="endddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="结束日期"/>
            <field id="reqstp" type="BaseEnumType.E_REQSTP" required="false" multi="false" array="false" longname="请求类型"/>
            <field id="selttp" type="BaseType.U_CHAR01" required="false" multi="false" array="false" longname="查询类型" desc="1-一个月,2-7天,3-其他"/>
        </input>
        <output asParm="true" packMode="false">
            <fields id="cplPredCabrInfos" scope="DpProdSvc.AcctPredCabrInfos" required="false" multi="false" longname="每天计提明细集合" array="false">
                <field id="cplPreCabrInfo" type="DpProdSvc.AcctPredCabrInfo" required="false" multi="true" array="false" longname="每天计提信息"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="DpAcctSvcType.SelAcctPredCabrForSeven" id="selAcctPredCabr" longname="查询智能储蓄每天计提利息" desc="查询智能储蓄每天计提利息">
            <in_mappings by_interface="true">
                <mapping src="custac" dest="custac" desc="电子账号" by_interface="true" on_top="true"/>
                <mapping src="bigndt" dest="bigndt" desc="起始日期" by_interface="true" on_top="true"/>
                <mapping src="endddt" dest="endddt" desc="结束日期" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="cplPreCabrInfos.cplPreCabrInfo" dest="cplPredCabrInfos.cplPreCabrInfo" desc="每天计提明细信息" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
