<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="qyacno" longname="查询子账户列表" kind="1" package="cn.sunline.edsp.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.edsp.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="acalno" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="账户别名"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="acctnoList" type="DpFrozType.CustAcctInfo" required="false" multi="true" array="false" longname="子账户列表"/>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service mappingToProperty="false" serviceName="IoDpFrozSvcType.loQueryAcctList" id="loQueryAcctList" longname="查询子账户列表">
            <in_mappings by_interface="true">
                <mapping src="acalno" dest="acalno" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="acctnoList" dest="acctnoList" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
