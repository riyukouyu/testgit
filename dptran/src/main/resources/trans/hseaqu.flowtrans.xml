<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="hseaqu" longname="活期智能存款交易明细查询" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="负债账号" ref="DpDict.Acct.acctno"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
            <field id="detltp" type="BaseEnumType.E_DETLTP" required="false" multi="false" array="false" longname="明细类型"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" ref="DpDict.Acct.cardno"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="hqtrdt" type="IoDpTable.IoDpTranDetlHqQry" required="false" multi="true" array="false" longname="活期明细查询"/>
        </output>
        <property packMode="true">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
            <field id="pagesz" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页容量"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="qryTranInfo" id="qryTranInfo" longname="交易前检查"/>
        <service serviceName="DpAcctSvcType.selHqTranDetl" id="selHqTranDetl" longname="活期智能存款交易明细查询">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cardno" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="crcycd" by_interface="true" on_top="true"/>
                <mapping src="detltp" dest="detltp" by_interface="true" on_top="true"/>
                <mapping src="pageno" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="pagesz" dest="pagesz" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="hqtrdt" dest="hqtrdt" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
