                <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="PbDict.Brch.brchno"/>
                <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编号" ref="DpDict.Prod.prodcd"/>
                <field id="intrcd" type="BaseType.U_INRTNO" required="false" multi="false" array="false" longname="利率编号" ref="DpDict.Prod.intrcd"/>
                <field id="intrna" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="利率名称" ref="PbDict.Intr.intrna"/>
                <field id="prodtx" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="产品名称"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoProRateSelSvr.ProRateSelSvr" id="ProRateSelSvr" longname="金融基础产品利率代码查询" desc="金融基础产品利率代码查询">
            <in_mappings by_interface="true">
                <mapping src="brchno" dest="brchno" by_interface="true" on_top="true"/>
                <mapping src="intrcd" dest="intrcd" by_interface="true" on_top="true"/>
                <mapping src="prodcd" dest="prodcd" by_interface="true" on_top="true"/>
