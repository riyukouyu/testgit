            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="result" dest="result" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
