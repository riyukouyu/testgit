<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="dpstfz" longname="系统冻结" kind="2" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[系统冻结]]></description>
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="货币代号" ref="DpDict.Acct.crcycd"/>
            <field id="frozam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="冻结金额" ref="DpDict.Froz.frozam"/>
            <field id="froztp" type="DpEnumType.E_FROZTP" required="false" multi="false" array="false" longname="冻结业务类型" ref="DpDict.Froz.froztp"/>
            <field id="sprdid" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="可售产品ID"/>
            <field id="sprdvr" type="BaseType.U_CHAR16" required="false" multi="false" array="false" longname="版本号"/>
            <field id="sprdna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="可售产品名称"/>
            <field id="ftrate" type="BaseType.U_INSTRT" required="false" multi="false" array="false" longname="预期年化收益率（最低）"/>
            <field id="ttrate" type="BaseType.U_INSTRT" required="false" multi="false" array="false" longname="预期年化收益率（最高）"/>
            <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品代码"/>
            <field id="endday" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="理财到期日"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="frozdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="冻结日期" ref="DpDict.Froz.frozdt"/>
            <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水" ref="DpDict.Froz.mntrsq"/>
            <field id="remark" type="BaseType.U_SHORTS" required="false" multi="false" array="false" longname="备注" ref="DpDict.Froz.remark"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoDpFrozSvcType.IoDpStFz" id="IoDpStFz" longname="系统冻结" desc="系统冻结">
            <in_mappings by_interface="true">
                <mapping src="cardno" dest="cpliodpstfzin.cardno" by_interface="true" on_top="true"/>
                <mapping src="crcycd" dest="cpliodpstfzin.crcycd" by_interface="true" on_top="true"/>
                <mapping src="endday" dest="cpliodpstfzin.endday" by_interface="true" on_top="true"/>
                <mapping src="frozam" dest="cpliodpstfzin.frozam" by_interface="true" on_top="true"/>
                <mapping src="froztp" dest="cpliodpstfzin.froztp" by_interface="true" on_top="true"/>
                <mapping src="ftrate" dest="cpliodpstfzin.ftrate" by_interface="true" on_top="true"/>
                <mapping src="prodcd" dest="cpliodpstfzin.prodcd" by_interface="true" on_top="true"/>
                <mapping src="sprdid" dest="cpliodpstfzin.sprdid" by_interface="true" on_top="true"/>
                <mapping src="sprdna" dest="cpliodpstfzin.sprdna" by_interface="true" on_top="true"/>
                <mapping src="sprdvr" dest="cpliodpstfzin.sprdvr" by_interface="true" on_top="true"/>
                <mapping src="ttrate" dest="cpliodpstfzin.ttrate" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="cpliodpstfzot.frozdt" dest="frozdt" by_interface="true" on_top="true"/>
                <mapping src="cpliodpstfzot.mntrsq" dest="mntrsq" by_interface="true" on_top="true"/>
                <mapping src="cpliodpstfzot.remark" dest="remark" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
        <method method="saveFnbProd" id="saveFnbProd" longname="购买理财产品登记"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
