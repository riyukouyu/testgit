<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="cuasqu" longname="存款资产查询" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="电子账号"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="存款子账号"/>
            <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编号"/>
            <field id="pddpfg" type="DpEnumType.E_FCFLAG" required="false" multi="false" array="false" longname="定活期标识"/>
            <field id="sprdid" type="BaseType.U_CHAR20" required="false" multi="false" array="false" longname="可售产品ID"/>
            <field id="sprdna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="可售产品名称"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="totbal" type="LnBaseType.U_ACBLTP" required="false" multi="false" array="false" longname="存款总额"/>
            <field id="curbal" type="LnBaseType.U_ACBLTP" required="false" multi="false" array="false" longname="活期总额"/>
            <field id="fxabal" type="LnBaseType.U_ACBLTP" required="false" multi="false" array="false" longname="定期总额"/>
            <field id="iscurr" type="string" required="false" multi="false" array="false" longname="是否有签约活期"/>
            <field id="lsisnl" type="string" required="false" multi="false" array="false" longname="存款列表是否为空"/>
            <field id="pddetl" type="IoDpTable.IoDpDpAssetsQry" required="false" multi="true" array="false" longname="存款资产明细"/>
        </output>
        <property packMode="true">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
            <field id="pagesize" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="每页记录数"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="qryTranInfo" id="qryTranInfo" longname="交易前处理"/>
        <service serviceName="DpAcctSvcType.selDqCuasqu" id="selDqCuasqu" longname="存款资产查询">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="cardno" dest="cardno" by_interface="true" on_top="true"/>
                <mapping src="pageno" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="pagesize" dest="pagesize" by_interface="true" on_top="true"/>
                <mapping src="pddpfg" dest="pddpfg" desc="定活期标识" by_interface="true" on_top="true"/>
                <mapping src="prodcd" dest="prodcd" by_interface="true" on_top="true"/>
                <mapping src="sprdid" dest="sprdid" desc="可售产品ID" by_interface="true" on_top="true"/>
                <mapping src="sprdna" dest="sprdna" desc="可售产品名称" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="curbal" dest="curbal" by_interface="true" on_top="true"/>
                <mapping src="fxabal" dest="fxabal" by_interface="true" on_top="true"/>
                <mapping src="iscurr" dest="iscurr" by_interface="true" on_top="true"/>
                <mapping src="lsisnl" dest="lsisnl" by_interface="true" on_top="true"/>
                <mapping src="pddetl" dest="pddetl" by_interface="true" on_top="true"/>
                <mapping src="totbal" dest="totbal" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
