<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrsaif" longname="产品销售情况查询" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="prodcd" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="产品代码" ref="PbDict.Dcmt.prodcd"/>
            <field id="plstad" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="计划开始日期" ref="DpDict.Acct.plstad"/>
            <field id="ploved" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="计划结束日期" ref="DpDict.Acct.ploved"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="lstKupDppbSalInfo" scope="DpProdConfType.KupDppbSalInfo" required="false" multi="true" longname="产品销售情况输出" array="false">
                <field id="prodcd" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="产品编号" ref="DpDict.Prod.prodcd"/>
                <field id="prodtx" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="产品名称" ref="DpDict.Prod.prodtx"/>
                <field id="fenshu" type="PbBaseType.U_FENSHU" required="false" multi="false" array="false" longname="份数" ref="PbDict.Dcmt.fenshu"/>
                <field id="totamt" type="PbBaseType.U_TOTLAM" required="false" multi="false" array="false" longname="合计金额" ref="PbDict.Charge.totamt"/>
            </fields>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <service serviceName="DpProdSvcType.QryPorSalInfo" longname="产品销售情况查询">
            <in_mappings by_interface="true">
                <mapping src="ploved" dest="ploved" by_interface="true" on_top="true"/>
                <mapping src="plstad" dest="plstad" by_interface="true" on_top="true"/>
                <mapping src="prodcd" dest="prodcd" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="lstKupDppbSalInfo" dest="lstKupDppbSalInfo" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
