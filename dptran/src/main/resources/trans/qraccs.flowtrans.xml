<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qraccs" longname="关联已有账户" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户卡号"/>
            <field id="passwd" type="BaseType.U_TRANPW" required="false" multi="false" array="false" longname="交易密码" ref="ApDict.Aplt.passwd"/>
            <field id="teleno" type="BaseType.U_TELENO" required="false" multi="false" array="false" longname="电话号码" ref="DpDict.Acct.teleno"/>
            <field id="pswdad" type="BaseType.U_CHAR180" required="false" multi="false" array="false" longname="客户端加密随机数"/>
            <field id="idtftp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="证件类型" ref="DpDict.Acct.idtftp"/>
            <field id="idtfno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="证件号码" ref="DpDict.Acct.idtfno"/>
            <field id="custna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="客户名称" ref="DpDict.Acct.custna"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="accatp" type="BaseEnumType.E_ACCATP" required="false" multi="false" array="false" longname="账户类型"/>
            <fields id="bindCardList" scope="IoCaTypeGenBindCard.IoCaBindCardInfo" required="false" multi="true" longname="绑定卡信息列表" array="false">
                <field id="cdopac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="绑定账户" ref="CaDict.Eacct.cdopac"/>
                <field id="cdopna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="绑定账户名称" ref="CaDict.Eacct.cdopna"/>
                <field id="opbrch" type="BaseType.U_CHAR20" required="false" multi="false" array="false" longname="账户开户行号" ref="CaDict.Eacct.opbrch"/>
                <field id="brchna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="账户开户行名称" ref="CaDict.Eacct.brchna"/>
            </fields>
        </output>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
    <flow>
        <method method="qraccs" id="qraccs" longname="关联已有账户" desc="查询客户在银行开立的所有正常有效的一类、二类户信息"/>
    </flow>
</flowtran>
