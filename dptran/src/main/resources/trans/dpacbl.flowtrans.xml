<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="dpacbl" longname="查询电子账户存款类账户交易明细(柜面)" kind="1" package="cn.sunline.ltts.busi.dptran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.intf">
        <input packMode="true">
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="负债账号" ref="DpDict.Acct.acctno"/>
            <field id="dptptl" type="CaEnumType.E_PDTPDL" required="false" multi="false" array="false" longname="产品类型细分"/>
            <field id="qractp" type="ApBaseEnumType.E_SETYPE" required="false" multi="false" array="false" longname="查询类型"/>
            <field id="amntcd" type="BaseEnumType.E_AMNTCD" required="false" multi="false" array="false" longname="借贷标志" ref="BaseDict.Comm.amntcd"/>
            <field id="bgindt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始日期"/>
            <field id="endddt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="终止日期"/>
            <field id="mxtram" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="最大交易金额"/>
            <field id="smrycd" type="BaseType.U_SMRYCD" required="false" multi="false" array="false" longname="摘要代码" ref="BaseDict.Comm.smrycd"/>
            <field id="opcuac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="对方账号"/>
            <field id="opcuna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="对方户名"/>
            <field id="mntram" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="最小交易金额"/>
            <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号" />
        </input>
        <output asParm="true" packMode="true">
            <field id="acctblList" type="IoDpTable.IoDpAcctblList" required="false" multi="true" array="false" longname="账户交易明细"/>
        </output>
        <property packMode="true">
            <field id="pageno" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="当前页数"/>
            <field id="pagesz" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="页容量"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="queryTran" id="queryTran" longname="交易前检查"/>
        <service serviceName="DpAcctSvcType.selDpDpacbl" id="selDpDpacbl" longname="查询电子账户存款类账户交易明细(柜面)">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="amntcd" dest="amntcd" by_interface="true" on_top="true"/>
                <mapping src="bgindt" dest="bgindt" by_interface="true" on_top="true"/>
                <mapping src="dptptl" dest="dptptl" by_interface="true" on_top="true"/>
                <mapping src="endddt" dest="endddt" by_interface="true" on_top="true"/>
                <mapping src="mntram" dest="mntram" by_interface="true" on_top="true"/>
                <mapping src="mxtram" dest="mxtram" by_interface="true" on_top="true"/>
                <mapping src="opcuac" dest="opcuac" by_interface="true" on_top="true"/>
                <mapping src="opcuna" dest="opcuna" by_interface="true" on_top="true"/>
                <mapping src="pageno" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="pagesz" dest="pagesz" by_interface="true" on_top="true"/>
                <mapping src="qractp" dest="qractp" by_interface="true" on_top="true"/>
                <mapping src="smrycd" dest="smrycd" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="acctblList" dest="acctblList" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
