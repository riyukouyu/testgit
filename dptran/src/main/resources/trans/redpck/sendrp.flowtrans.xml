<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="sendrp" longname="发红包" kind="3" package="cn.sunline.ltts.busi.dptran.trans.redpck" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.dptran.trans.redpck.intf">
        <input packMode="true">
            <field id="rptrtp" type="DpEnumType.E_RPTRTP" required="true" multi="false" array="false" longname="红包交易类型"/>
            <field id="otcard" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转出方卡号/账号"/>
            <field id="otcstp" type="BaseEnumType.E_ACSETP" required="false" multi="false" array="false" longname="子账户类型"/>
            <field id="otbrch" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="转出方机构号"/>
            <field id="otname" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="客户名称"/>
            <field id="userid" type="BaseType.U_CUSTID" required="false" multi="false" array="false" longname="用户ID"/>
            <field id="rpcode" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="红包代码"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" required="true" multi="false" array="false" longname="币种"/>
            <field id="csextg" type="BaseEnumType.E_CSEXTG" required="true" multi="false" array="false" longname="钞汇标志"/>
            <field id="tranam" type="BaseType.U_TRANAM" required="true" multi="false" array="false" longname="交易金额"/>
            <field id="chckdt" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="对账日期"/>
            <field id="keepdt" type="BaseType.U_DATE08" required="true" multi="false" array="false" longname="清算日期"/>
            <field id="smrycd" type="BaseType.U_SMRYCD" required="false" multi="false" array="false" longname="摘要代码" ref="BaseDict.Comm.smrycd"/>
            <field id="stady1" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="备用字段1"/>
            <field id="stady2" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="备用字段2"/>
            <field id="soursq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="来源方交易流水"/>
            <field id="acctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="账务日期" ref="DpDict.Acct.acctdt"/>
        </input>
        <output asParm="true" packMode="true">
            <field id="mntrsq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="主交易流水"/>
            <field id="mntrdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="主交易日期"/>
            <field id="mntrtm" type="BaseType.U_TRTMCH" required="false" multi="false" array="false" longname="主交易时间"/>
            <field id="caccno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="贷方内部户账号"/>
            <field id="daccno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="借方内部户账号"/>
            <field id="caccna" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="贷方内部户名称"/>
            <field id="daccna" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="借方内部户名称"/>
        </output>
        <property packMode="true">
            <field id="custac" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="电子账号ID"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="子账号"/>
            <field id="auacfg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="存取标志"/>
            <field id="acbrno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="内部户帐号"/>
            <field id="acbrna" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="内部户帐号名称"/>
            <field id="amntcd" type="BaseEnumType.E_AMNTCD" required="false" multi="false" array="false" longname="借贷标志"/>
            <field id="trantp" type="BaseEnumType.E_TRANTP" required="false" multi="false" array="false" longname="交易类型"/>
            <field id="acbrch" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="账户机构"/>
            <field id="busino" type="BaseType.U_BUSINO" required="false" multi="false" array="false" longname="业务编码IA"/>
            <field id="dscrtx" type="BaseType.U_CHAR200" required="false" multi="false" array="false" longname="描述"/>
            <field id="isacsc" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否从电子账户支出"/>
            <field id="reacno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="收方对方内部户账号"/>
            <field id="reacna" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="收方对方内部户账号名称"/>
            <field id="inacno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="转入内部户帐号"/>
            <field id="inacna" type="BaseType.U_CHAR50" required="false" multi="false" array="false" longname="转入内部户帐号名称"/>
            <field id="intrtp" type="BaseEnumType.E_TRANTP" required="false" multi="false" array="false" longname="转入交易类型"/>
            <field id="inacbr" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="转入账户机构"/>
            <field id="inbusi" type="BaseType.U_BUSINO" required="false" multi="false" array="false" longname="转入业务编码IA"/>
            <field id="insmcd" type="BaseType.U_SMRYCD" required="false" multi="false" array="false" longname="转入摘要码"/>
            <field id="subsac" type="BaseType.U_CUACNO" required="false" multi="false" array="false" longname="子户号" ref="InDict.inac.subsac"/>
            <field id="servtp" type="MsType.U_CHANNEL" required="false" multi="false" array="false" longname="交易渠道"/>
            <field id="servdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="渠道日期"/>
            <field id="recpay" type="QtEnumType.E_RECPAY" required="false" multi="false" array="false" longname="收付方标识"/>
            <field id="custie" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否绑定卡"/>
        </property>
        <printer packMode="false"/>
    </interface>
    <flow>
        <method method="DealTransBefore" id="DealTransBefore" longname="交易前处理"/>
        <service serviceName="IoCaSevAccountLimit.IoAcSubQuota" id="IoAcSubQuota" test="rptrtp == #dict.DpEnumType.E_RPTRTP().SN103 || rptrtp == #dict.DpEnumType.E_RPTRTP().SN104" longname="电子账户系统扣减客户额度">
            <in_mappings by_interface="true">
                <mapping src="servdt" dest="servdt" by_interface="true" on_top="true"/>
                <mapping src="servtp" dest="servtp" by_interface="true" on_top="true"/>
                <mapping src="tranam" dest="tranam" by_interface="true" on_top="true"/>
                <mapping src="recpay" dest="recpay" desc="description" by_interface="true" on_top="true"/>
                <mapping src="custie" dest="custie" desc="description" by_interface="true" on_top="true"/>
                <mapping src="custac" dest="custac" desc="description" by_interface="true" on_top="true"/>
                <mapping src="soursq" dest="servsq" desc="description" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true"/>
        </service>
        <case id="case_1" longname="判断发红包类型">
            <when id="when_1" test="rptrtp == #dict.DpEnumType.E_RPTRTP().SN101 || rptrtp == #dict.DpEnumType.E_RPTRTP().SN102" longname="行社红包">
                <service mappingToProperty="true" serviceName="IoInAccount.IoInAcdr" id="IoInAcdr" longname="内部户借方记账服务" desc="借：30410306网络柜面核心间往来">
                    <in_mappings by_interface="true">
                        <mapping src="acbrch" dest="input.acbrch" by_interface="true" on_top="true"/>
                        <mapping src="acbrno" dest="input.acctno" by_interface="true" on_top="true"/>
                        <mapping src="amntcd" dest="input.amntcd" by_interface="true" on_top="true"/>
                        <mapping src="busino" dest="input.busino" by_interface="true" on_top="true"/>
                        <mapping src="crcycd" dest="input.crcycd" by_interface="true" on_top="true"/>
                        <mapping src="dscrtx" dest="input.dscrtx" by_interface="true" on_top="true"/>
                        <mapping src="smrycd" dest="input.smrycd" by_interface="true" on_top="true"/>
                        <mapping src="inacno" dest="input.toacct" by_interface="true" on_top="true"/>
                        <mapping src="inacna" dest="input.toacna" desc="description" by_interface="true" on_top="true"/>
                        <mapping src="tranam" dest="input.tranam" by_interface="true" on_top="true"/>
                        <mapping src="trantp" dest="input.trantp" by_interface="true" on_top="true"/>
                    </in_mappings>
                    <out_mappings by_interface="true">
                        <mapping src="output.acctno" dest="acbrno" by_interface="true" on_top="true"/>
                        <mapping src="output.acctna" dest="acbrna" by_interface="true" on_top="true"/>
                    </out_mappings>
                </service>
                <service mappingToProperty="true" serviceName="IoInAccount.IoInAccr" id="IoInAccr" longname="内部户贷方记账服务">
                    <in_mappings by_interface="true">
                        <mapping src="inacbr" dest="input.acbrch" by_interface="true" on_top="true"/>
                        <mapping src="inbusi" dest="input.busino" by_interface="true" on_top="true"/>
                        <mapping src="crcycd" dest="input.crcycd" by_interface="true" on_top="true"/>
                        <mapping src="dscrtx" dest="input.dscrtx" by_interface="true" on_top="true"/>
                        <mapping src="reacno" dest="input.toacct" by_interface="true" on_top="true"/>
                        <mapping src="reacna" dest="input.toacna" desc="description" by_interface="true" on_top="true"/>
                        <mapping src="tranam" dest="input.tranam" by_interface="true" on_top="true"/>
                        <mapping src="intrtp" dest="input.trantp" by_interface="true" on_top="true"/>
                        <mapping src="smrycd" dest="input.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
                        <mapping src="subsac" dest="input.subsac" by_interface="true" on_top="true"/>
                    </in_mappings>
                    <out_mappings by_interface="true">
                        <mapping src="output.acctno" dest="inacno" by_interface="true" on_top="true"/>
                        <mapping src="output.acctna" dest="inacna" by_interface="true" on_top="true"/>
                    </out_mappings>
                </service>
                <method method="registerAccountting" id="registerAccountting" longname="内部户贷方登记会计流水"/>
            </when>
            <when id="when_2" test="rptrtp == #dict.DpEnumType.E_RPTRTP().SN103 || rptrtp == #dict.DpEnumType.E_RPTRTP().SN104" longname="商户或个人红包">
                <service serviceName="DpAcctSvcType.addDrawAcctDp" id="addDrawAcctDp" longname="电子账户支取记账服务">
                    <in_mappings by_interface="true">
                        <mapping src="acctno" dest="cplDrawAcctIn.acctno" by_interface="true" on_top="true"/>
                        <mapping src="acctno" dest="cplDrawAcctIn.acseno" by_interface="true" on_top="true"/>
                        <mapping src="auacfg" dest="cplDrawAcctIn.auacfg" by_interface="true" on_top="true"/>
                        <mapping src="otcard" dest="cplDrawAcctIn.cardno" by_interface="true" on_top="true"/>
                        <mapping src="crcycd" dest="cplDrawAcctIn.crcycd" by_interface="true" on_top="true"/>
                        <mapping src="custac" dest="cplDrawAcctIn.custac" by_interface="true" on_top="true"/>
                        <mapping src="stady1" dest="cplDrawAcctIn.linkno" by_interface="true" on_top="true"/>
                        <mapping src="inacna" dest="cplDrawAcctIn.opacna" by_interface="true" on_top="true"/>
                        <mapping src="inacno" dest="cplDrawAcctIn.toacct" by_interface="true" on_top="true"/>
                        <mapping src="tranam" dest="cplDrawAcctIn.tranam" by_interface="true" on_top="true"/>
                        <mapping src="smrycd" dest="cplDrawAcctIn.smrycd" desc="摘要码" by_interface="true" on_top="true"/>
                    </in_mappings>
                    <out_mappings by_interface="true"/>
                </service>
                <service mappingToProperty="true" serviceName="IoInAccount.IoInAccrAdm" id="IoInAccrAdm" longname="内部户贷方记账服务(外调)">
                    <in_mappings by_interface="true">
                        <mapping src="inacbr" dest="input.acbrch" by_interface="true" on_top="true"/>
                        <mapping src="inbusi" dest="input.busino" by_interface="true" on_top="true"/>
                        <mapping src="crcycd" dest="input.crcycd" by_interface="true" on_top="true"/>
                        <mapping src="dscrtx" dest="input.dscrtx" by_interface="true" on_top="true"/>
                        <mapping src="reacno" dest="input.toacct" by_interface="true" on_top="true"/>
                        <mapping src="reacna" dest="input.toacna" desc="description" by_interface="true" on_top="true"/>
                        <mapping src="tranam" dest="input.tranam" by_interface="true" on_top="true"/>
                        <mapping src="intrtp" dest="input.trantp" by_interface="true" on_top="true"/>
                        <mapping src="smrycd" dest="input.smrycd" desc="摘要代码" by_interface="true" on_top="true"/>
                        <mapping src="subsac" dest="input.subsac" by_interface="true" on_top="true"/>
                    </in_mappings>
                    <out_mappings by_interface="true">
                        <mapping src="output.acctno" dest="inacno" by_interface="true" on_top="true"/>
                        <mapping src="output.acctna" dest="inacna" by_interface="true" on_top="true"/>
                    </out_mappings>
                </service>
            </when>
        </case>
        <method method="DealTransAfter" id="DealTransAfter" longname="交易后处理"/>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
