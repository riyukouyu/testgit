<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="cnaphswt" longname="小额对账文件下载" kind="write" package="cn.sunline.ltts.busi.dptran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="cnaphswt" longname="小额对账文件下载" mode="splitor" splitor="|" encoding="UTF-8">
        <header id="Header" longname="文件头" startline="1">
            <field id="totanm" type="ApBaseType.U_CHANZXLX" longname="总笔数" leftPadding="false" mode="splitor"/>
        </header>
        <body id="Body" longname="文件体" mapping="DpDepoBusiMain.KnlCnapot" startline="2">
            <field id="iotype" type="BaseType.U_CHAR01" longname="往来标志" leftPadding="false" mode="splitor"/>
            <field id="msetdt" type="BaseType.U_DATE08" longname="委托日期" leftPadding="false" mode="splitor"/>
            <field id="msetsq" type="BaseType.U_CHAR12" longname="交易序号" leftPadding="false" mode="splitor"/>
            <field id="crdbtg" type="BaseType.U_CHAR01" longname="借贷标志" leftPadding="false" mode="splitor"/>
            <field id="mesgtp" type="BaseType.U_CHAR16" longname="报文编号" leftPadding="false" mode="splitor"/>
            <field id="pyercd" type="LnBaseType.U_BANKNO" longname="发起行行号" leftPadding="false" mode="splitor"/>
            <field id="pyerac" type="BaseType.U_CUSTAC" longname="付款人账号" leftPadding="false" mode="splitor"/>
            <field id="pyeecd" type="LnBaseType.U_BANKNO" longname="接收行行号" leftPadding="false" mode="splitor"/>
            <field id="pyeeac" type="BaseType.U_CUSTAC" longname="收款人账号" leftPadding="false" mode="splitor"/>
            <field id="tranam" type="BaseType.U_TRANAM" longname="发生额" leftPadding="false" mode="splitor"/>
            <field id="pakgdt" type="BaseType.U_DATE08" longname="包委托日期" leftPadding="false" mode="splitor"/>
            <field id="pakgsq" type="BaseType.U_CHAR12" longname="包标识序号" leftPadding="false" mode="splitor"/>
            <field id="keepdt" type="BaseType.U_DATE08" longname="清算日期" leftPadding="false" mode="splitor"/>
            <field id="npcpdt" type="BaseType.U_DATE08" longname="轧差日期" leftPadding="false" mode="splitor"/>
            <field id="npcpbt" type="BaseType.U_DATE08" longname="轧差场次" leftPadding="false" mode="splitor"/>
            <field id="reproc" type="BaseType.U_CHAR12" longname="原交易码" leftPadding="false" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="文件批次号"/>
            <field id="servdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="文件日期"/>
            <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="对账日期"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="neibjlid" type="long" required="false" multi="false" array="false" longname="内部处理ID"/>
            <field id="taskid" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="批量交易批次号" ref="ApDict.Aplt.taskid"/>
            <field id="fhwjmche" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="回盘文件名称"/>
            <field id="fhwjbdlj" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="回盘文件目录"/>
            <field id="dwpath" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="文件路径"/>
        </property>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
