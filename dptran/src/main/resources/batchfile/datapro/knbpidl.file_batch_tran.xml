<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="knbpidl" longname="会计流水清算表数仓抽数" kind="write" package="cn.sunline.ltts.busi.dptran.batchfile.datapro" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="knbpidl" longname="负债账户付息明细表数仓抽数" mode="fixAndSplitor" splitor="&amp;" splitorAsEnd="true" encoding="GBK">
        <header id="Header" longname="文件头" mapping="SysFileTables.kapb_wjplxxb" startline="1"/>
        <body id="Body" longname="文件体" mapping="java.util.Map" mode="fixAndSplitor" splitor="&amp;" startline="1">
            <field id="corpno" type="BaseType.U_CORPNO" longname="法人代码" length="6" leftPadding="false" padding=" " ref="ApDict.Aplt.corpno"/>
            <field id="acctno" type="BaseType.U_ACCTNO" longname="负债账号" length="80" leftPadding="false" padding=" " ref="DpDict.Acct.acctno"/>
            <field id="acsetp" type="BaseEnumType.E_ACSETP" longname="子账户类型" length="4" leftPadding="false" padding=" "/>
            <field id="intrtp" type="DpEnumType.E_INTRTP" longname="利息类型" length="16" leftPadding="false" padding=" " ref="DpDict.Prod.intrtp"/>
            <field id="indxno" type="BaseType.U_LONGIN" longname="顺序号" length="19" leftPadding="false" padding=" "/>
            <field id="detlsq" type="BaseType.U_LONGIN" longname="明细序号" length="19" leftPadding="false" padding=" " ref="DpDict.Acct.detlsq"/>
            <field id="indlst" type="BaseEnumType.E_INDLST" longname="负债利息明细状态" length="2" leftPadding="false" padding=" " ref="DpDict.Prod.indlst"/>
            <field id="lsinoc" type="BaseEnumType.E_INDLTP" longname="上次利息操作代码" length="20" leftPadding="false" padding=" " ref="DpDict.Prod.lsinoc"/>
            <field id="instdt" type="BaseType.U_DATE08" longname="计息起始日期" length="16" leftPadding="false" padding=" "/>
            <field id="ineddt" type="BaseType.U_DATE08" longname="计息终止日期" length="16" leftPadding="false" padding=" "/>
            <field id="intrcd" type="BaseType.U_INRTNO" longname="利率编号" length="40" leftPadding="false" padding=" " ref="DpDict.Prod.intrcd"/>
            <field id="incdtp" type="DpEnumType.E_IRCDTP" longname="利率代码类型" length="2" leftPadding="false" padding=" " ref="DpDict.Prod.incdtp"/>
            <field id="lyinwy" type="DpEnumType.E_LYINWY" longname="分层计息方式" length="2" leftPadding="false" padding=" " ref="DpDict.Prod.lyinwy"/>
            <field id="intrwy" type="DpEnumType.E_IRDPWY" longname="利率靠档方式" length="2" leftPadding="false" padding=" " ref="DpDict.Prod.intrwy"/>
            <field id="lvamot" type="BaseType.U_TRANAM" longname="分层金额" length="17" leftPadding="false" padding=" "/>
            <field id="lvindt" type="BaseEnumType.E_TERMCD" longname="层次利率存期" length="12" leftPadding="false" padding=" "/>
            <field id="gradin" type="BaseType.U_TRANAM" longname="档次计息余额" length="17" leftPadding="false" padding=" " desc="当进行利息计提时，对于利率分档计息的，存放用于对应档次计息的计息金额；对于不分档计息的，存放该段的计息余额；对于利息操作代码为“利息调整”的，则为0；"/>
            <field id="totlin" type="BaseType.U_TRANAM" longname="总计息余额" length="17" leftPadding="false" padding=" " desc="该时间段用于计息的总余额，相当于同序号下档次计息余额相加，计提、调整时，是发生计提、调整的本金余额；对于利息支付；对于利息操作代码为“利息调整”的，则为0；"/>
            <field id="acmltn" type="BaseType.U_ACMLBL" longname="积数" length="21" leftPadding="false" padding=" "/>
            <field id="txbebs" type="DpEnumType.E_INBEBS" longname="计息基础" length="4" leftPadding="false" padding=" " ref="DpDict.Prod.txbebs"/>
            <field id="acbsin" type="BaseType.U_INSTRT" longname="账户基准利率" length="9" leftPadding="false" padding=" " ref="DpDict.Prod.acbsin"/>
            <field id="cuusin" type="BaseType.U_INSTRT" longname="当前执行利率" length="9" leftPadding="false" padding=" " ref="DpDict.Prod.cuusin"/>
            <field id="rlintr" type="BaseType.U_TOTAIN" longname="实际利息发生额" length="24" leftPadding="false" padding=" " desc="发生额为正表示利息的增加，发生额为负表示利息的减少。"/>
            <field id="rlintx" type="BaseType.U_TOTAIN" longname="实际利息税发生额" length="24" leftPadding="false" padding=" " desc="发生额为正表示利息税的增加，发生额为负表示利息税的减少。"/>
            <field id="intrdt" type="BaseType.U_DATE08" longname="计息日期" length="16" leftPadding="false" padding=" "/>
            <field id="intrsq" type="BaseType.U_SEQUNO" longname="计息流水" length="64" leftPadding="false" padding=" "/>
            <field id="pyindt" type="BaseType.U_DATE08" longname="付息日期" length="16" leftPadding="false" padding=" "/>
            <field id="pyinsq" type="BaseType.U_SEQUNO" longname="付息流水" length="64" leftPadding="false" padding=" "/>
            <field id="remark" type="BaseType.U_SMRYDS" longname="靠档利率" length="160" leftPadding="false" padding=" "/>
            <field id="tmstmp" type="ApBaseType.U_TMSTMP" longname="时间戳" length="42" leftPadding="false" padding=" " ref="ApDict.Aplt.tmstmp"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.datapro.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="批量交易批次号"/>
            <field id="acctdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期"/>
            <field id="busseq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="业务流水"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true"/>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
