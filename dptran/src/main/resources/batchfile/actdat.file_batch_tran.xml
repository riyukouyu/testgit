<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="actdat" longname="营销活动写文件" kind="write" package="cn.sunline.ltts.busi.dptran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="actdat" longname="营销活动写文件" mode="splitor" splitor="|@|" encoding="UTF-8">
        <header id="Header" longname="文件头" mapping="SysFileTables.kapb_wjplxxb" startline="1">
            <field id="actiid" type="BaseType.U_CHAR100" longname="活动ID" leftPadding="false"/>
            <field id="cyendt" type="BaseType.U_DATE08" longname="周期结束时间" leftPadding="false"/>
            <field id="totals" type="BaseType.U_INTEGE" longname="总记录数" leftPadding="false"/>
        </header>
        <body id="Body" longname="文件体" mapping="CaCustInfo.DataOfActiid" startline="1">
            <field id="custid" type="BaseType.U_CUSTID" longname="用户ID" leftPadding="false" ref="BaseDict.Comm.custid"/>
            <field id="custna" type="BaseType.U_ACCTNA" longname="客户名称" leftPadding="false" ref="BaseDict.Comm.custna"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="批量交易批次号"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true">
            <field id="actiid" type="BaseType.U_CHAR100" required="false" multi="false" array="false" longname="活动ID"/>
            <field id="syunum" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="活动剩余名额"/>
            <field id="cystdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="周期开始时间"/>
            <field id="cyendt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="周期结束时间"/>
            <field id="kaihsj" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="开户天数"/>
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="机构号" ref="PbDict.Brch.brchno"/>
            <field id="totals" type="BaseType.U_INTEGE" required="false" multi="false" array="false" longname="总记录数"/>
        </property>
        <printer packMode="true"/>
    </interface>
</file_batch_transaction>
