<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="batranwt" longname="电子账户批量转账写文件" kind="write" package="cn.sunline.ltts.busi.dptran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="batranwt" longname="电子账户批量转账写文件" mode="splitor" splitor="|" encoding="UTF-8">
        <header id="Header" longname="文件头" mapping="SysFileTables.kapb_wjplxxb" startline="1"/>
        <body id="Body" longname="文件体" mapping="DpDepoBusiMain.KnbAcctBach" startline="2">
            <field id="accttp" type="BaseType.U_CHAR01" longname="归集账户类型" leftPadding="false" mode="splitor"/>
            <field id="acctno" type="BaseType.U_ACCTNO" longname="归集账号" leftPadding="false" mode="splitor"/>
            <field id="acctna" type="BaseType.U_ACCTNA" longname="归集账户名称" leftPadding="false" mode="splitor"/>
            <field id="pyacct" type="BaseType.U_CARDNO" longname="被归集账号" leftPadding="false" mode="splitor"/>
            <field id="pyacna" type="BaseType.U_ACCTNA" longname="被归集账户名称" leftPadding="false" mode="splitor"/>
            <field id="brchno" type="BaseType.U_BRCHNO" longname="被归集账户开户行机构" leftPadding="false" mode="splitor"/>
            <field id="sweptp" type="BaseType.U_CHAR01" longname="归集方式" leftPadding="false" mode="splitor"/>
            <field id="swpamt" type="BaseType.U_TRANAM" longname="归集金额" leftPadding="false" mode="splitor"/>
            <field id="transq" type="BaseType.U_SEQUNO" longname="主交易流水" leftPadding="false" ref="DpDict.Acct.transq" mode="splitor"/>
            <field id="trandt" type="BaseType.U_DATE08" longname="交易日期" leftPadding="false" ref="DpDict.Acct.trandt" mode="splitor"/>
            <field id="transt" type="BaseEnumType.E_TRANST" longname="交易状态" leftPadding="false" mode="splitor"/>
            <field id="descrb" type="BaseType.U_SHOTDS" longname="交易描述" leftPadding="false" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="true" multi="false" array="false" longname="批量交易批次号"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="dwname" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="文件路径"/>
            <field id="dwpath" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="文件名"/>
        </property>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
