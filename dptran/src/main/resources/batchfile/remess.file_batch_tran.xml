<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="remess" longname="读取客户提示信息" kind="read" package="cn.sunline.ltts.busi.dptran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="remess" longname="读取客户提示信息" mode="splitor" splitor="|@|" encoding="UTF-8">
        <header id="Header" longname="文件头" mapping="SysFileTables.kapb_wjplxxb" startline="1">
            <field id="totanm" type="BaseType.U_LONGIN" longname="总记录数" leftPadding="false" mode="splitor"/>
        </header>
        <body id="Body" longname="文件体" mapping="DpMessTable.KnpPromMessBatc" startline="2">
            <field id="cardno" type="BaseType.U_CARDNO" longname="交易卡号" leftPadding="false" ref="BaseDict.Comm.cardno"/>
            <field id="messno" type="BaseType.U_CHAR10" longname="信息序号" leftPadding="false"/>
            <field id="isprom" type="BaseEnumType.E_YES___" longname="是否标志" leftPadding="false"/>
            <field id="servtp" type="MsType.U_CHANNEL" longname="渠道" leftPadding="false" ref="BaseDict.Comm.servtp"/>
            <field id="messtp" type="DpEnumType.E_MESSTP" longname="消息类型" leftPadding="false"/>
            <field id="contxt" type="BaseType.U_CHAR1000" longname="消息内容" leftPadding="false"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="文件批次号"/>
        </input>
        <output asParm="true" packMode="true"/>
        <property packMode="true"/>
        <printer packMode="true"/>
    </interface>
</file_batch_transaction>
