<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="rpchozrd" longname="红包批量冲正上送文件" kind="read" package="cn.sunline.ltts.busi.dptran.batchfile.redpck" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="rpchozrd" longname="红包批量冲正交易读文件" mode="splitor" splitor="|@|" encoding="UTF-8">
        <header id="Header" longname="文件头" mapping="SysFileTables.kapb_wjplxxb" startline="1">
            <field id="totanm" type="BaseType.U_LONGIN" longname="总记录条数" leftPadding="false" mode="splitor"/>
            <field id="totamt" type="BaseType.U_TRANAM" longname="总金额" leftPadding="false" mode="splitor"/>
        </header>
        <body id="Body" longname="文件体" mapping="RpMgr.KnbRptrBach" startline="2">
            <field id="soursq" type="BaseType.U_SEQUNO" longname="来源方交易流水" leftPadding="false" mode="splitor"/>
            <field id="sourdt" type="BaseType.U_DATE08" longname="来源方交易日期" leftPadding="false" mode="splitor"/>
            <field id="rptrtp" type="DpEnumType.E_RPTRTP" longname="红包交易类型" leftPadding="false" mode="splitor"/>
            <field id="rptype" type="DpEnumType.E_RPTRTP" longname="红包类型" leftPadding="false" mode="splitor"/>
            <field id="smrycd" type="BaseType.U_SMRYCD" longname="摘要码" leftPadding="false" ref="BaseDict.Comm.smrycd" mode="splitor"/>
            <field id="debact" type="BaseType.U_CUSTAC" longname="借方账号" leftPadding="false" mode="splitor"/>
            <field id="decstp" type="BaseEnumType.E_ACSETP" longname="借方子账户类型" leftPadding="false" mode="splitor"/>
            <field id="deborg" type="BaseType.U_BRCHNO" longname="借方机构" leftPadding="false" mode="splitor"/>
            <field id="debnam" type="BaseType.U_ACCTNA" longname="借方户名" leftPadding="false" mode="splitor"/>
            <field id="userid" type="BaseType.U_SEQUNO" longname="用户ID" leftPadding="false" mode="splitor"/>
            <field id="rpcode" type="BaseType.U_SEQUNO" longname="红包编号" leftPadding="false" mode="splitor"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" longname="货币代号" leftPadding="false" ref="DpDict.Acct.crcycd" mode="splitor"/>
            <field id="csextg" type="BaseEnumType.E_CSEXTG" longname="钞汇标志" leftPadding="false" mode="splitor"/>
            <field id="tranam" type="BaseType.U_TRANAM" longname="交易金额" leftPadding="false" ref="DpDict.Acct.tranam" mode="splitor"/>
            <field id="oriseq" type="BaseType.U_SEQUNO" longname="原交易流水" leftPadding="false" mode="splitor"/>
            <field id="oridat" type="BaseType.U_DATE08" longname="原交易日期" leftPadding="false" mode="splitor"/>
            <field id="stady1" type="BaseType.U_SHORDS" longname="备用字段1" leftPadding="false" mode="splitor"/>
            <field id="stady2" type="BaseType.U_SHORDS" longname="备用字段2" leftPadding="false" mode="splitor"/>
            <field id="stady3" type="BaseType.U_SHORDS" longname="备用字段3" leftPadding="false" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.redpck.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_SEQUNO" required="true" multi="false" array="false" longname="文件批次号"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="filena" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="文件名"/>
            <field id="dwpath" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="文件路径"/>
        </property>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
