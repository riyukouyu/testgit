<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="rpchckwt" longname="红包对账交易写文件" kind="write" package="cn.sunline.ltts.busi.dptran.batchfile.redpck" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="rpchckwt" longname="红包对账交易回传文件" mode="splitor" splitor="|" encoding="UTF-8">
        <header id="Header" longname="文件头" mapping="SysFileTables.kapb_wjplxxb" startline="1">
            <field id="zongbish" type="ApBaseType.U_CHANZXLX" longname="总笔数" leftPadding="false" mode="splitor"/>
            <field id="chulizbs" type="ApBaseType.U_CHANZXLX" longname="处理总笔数" leftPadding="false" mode="splitor"/>
            <field id="chenggbs" type="ApBaseType.U_CHANZXLX" longname="成功数" leftPadding="false" mode="splitor"/>
            <field id="shibaibs" type="ApBaseType.U_CHANZXLX" longname="失败数" leftPadding="false" mode="splitor"/>
        </header>
        <body id="Body" longname="文件体" mapping="RpMgr.KnbRptrDetl" startline="2">
            <field id="soursq" type="BaseType.U_SEQUNO" longname="来源方交易流水" leftPadding="false" mode="splitor"/>
            <field id="sourdt" type="BaseType.U_DATE08" longname="来源方交易日期" leftPadding="false" mode="splitor"/>
            <field id="rptrtp" type="DpEnumType.E_RPTRTP" longname="红包交易类型" leftPadding="false" mode="splitor"/>
            <field id="userid" type="BaseType.U_SEQUNO" longname="用户ID" leftPadding="false" mode="splitor"/>
            <field id="rpcode" type="BaseType.U_SEQUNO" longname="红包编号" leftPadding="false" mode="splitor"/>
            <field id="decard" type="PbBaseType.U_CUSTAC" longname="借方卡号" leftPadding="false" mode="splitor"/>
            <field id="debnam" type="BaseType.U_ACCTNA" longname="借方户名" leftPadding="false" mode="splitor"/>
            <field id="crcard" type="BaseType.U_CUSTAC" longname="贷方卡号" leftPadding="false" mode="splitor"/>
            <field id="crdnam" type="BaseType.U_ACCTNA" longname="贷方名称" leftPadding="false" mode="splitor"/>
            <field id="tranam" type="BaseType.U_TRANAM" longname="交易金额" leftPadding="false" mode="splitor"/>
            <field id="deborg" type="BaseType.U_BRCHNO" longname="借方机构" leftPadding="false" mode="splitor"/>
            <field id="crdorg" type="BaseType.U_BRCHNO" longname="贷方机构" leftPadding="false" mode="splitor"/>
            <field id="transq" type="BaseType.U_SEQUNO" longname="主交易流水" leftPadding="false" mode="splitor"/>
            <field id="trandt" type="BaseType.U_DATE08" longname="主交易日期" leftPadding="false" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.redpck.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="批量交易批次号"/>
            <field id="chckdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="对账日期"/>
            <field id="keepdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="清算日期"/>
            <field id="busseq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="业务流水"/>
            <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="交易日期"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="dwname" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="文件名"/>
            <field id="dwpath" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="文件路径"/>
        </property>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
