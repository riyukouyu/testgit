<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="batbilrd" longname="信用卡批量还款读文件" kind="read" package="cn.sunline.ltts.busi.dptran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="batbilrd" longname="信用卡批量还款读文件" mode="fix" encoding="GBK">
        <header id="Header" longname="文件头" startline="1"/>
        <body id="Body" longname="文件体" mapping="RpMgr.KnbCrcdBach" mode="fix" startline="1">
            <field id="bankcd" type="BaseType.U_CHAR04" longname="银行编号" length="4" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="bankno" type="BaseType.U_CHAR04" longname="分行代号" length="4" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="regidt" type="BaseType.U_DATE08" longname="记录产生日期" length="8" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="cardno" type="BaseType.U_CHAR20" longname="扣账账户号码" length="20" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="dealms" type="BaseType.U_CHAR12" longname="交易金额(临时保存)" length="12" leftPadding="true" padding="'0'" mode="splitor"/>
            <field id="trancy" type="BaseType.U_CHAR03" longname="扣账币种" length="3" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="dealdt" type="BaseType.U_DATE08" longname="扣账日期" length="8" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="crcard" type="BaseType.U_CHAR19" longname="信用卡卡号" length="19" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="cridtp" type="BaseType.U_CHAR02" longname="证件类型" length="2" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="cridno" type="BaseType.U_CHAR18" longname="证件号码" length="18" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="cractp" type="BaseType.U_CHAR01" longname="扣款账户类型" length="1" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="ordrno" type="BaseType.U_CHAR04" longname="排序优先级" length="4" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="spancd" type="BaseType.U_CHAR04" longname="跨行自扣账号" length="4" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="stanby" type="BaseType.U_CHAR02" longname="保留字段" length="2" leftPadding="false" padding="' '" mode="splitor"/>
            <field id="cracna" type="BaseType.U_CHAR62" longname="姓名" length="62" leftPadding="false" padding="' '" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="文件批次号"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="dwname" type="BaseType.U_SHORDS" required="false" multi="false" array="false" longname="文件名称"/>
            <field id="dwpath" type="BaseType.U_LONGDS" required="false" multi="false" array="false" longname="文件路径"/>
        </property>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
