<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="agentPaidR" longname="批量代发读文件" kind="read" package="cn.sunline.edsp.busi.dptran.batchfile.ca" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
