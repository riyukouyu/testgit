            <field id="totlnm" type="BaseType.U_LONGIN" longname="数量" leftPadding="false" ref="PbDict.Cash.totlnm"/>
            <field id="totlam" type="PbBaseType.U_DEC212" longname="总金额" leftPadding="false" ref="PbDict.Intr.totlam"/>
        </header>
        <body id="Body" longname="文件体" mapping="DpDepoBusiMain.knb_batch_profit" startline="2">
            <field id="transq" type="BaseType.U_SEQUNO" longname="交易流水" leftPadding="false" ref="BaseDict.Comm.transq"/>
            <field id="trandt" type="BaseType.U_DATE08" longname="交易日期" leftPadding="false" ref="BaseDict.Comm.trandt"/>
            <field id="cltrsq" type="BaseType.U_CHAR32" longname="清结算流水" leftPadding="false" ref="DpRiskDict.JfDp.cltrsq"/>
