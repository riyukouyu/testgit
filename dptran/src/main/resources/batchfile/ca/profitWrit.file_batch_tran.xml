            <field id="jftrtp" type="JfDpEnumType.E_JFTRTP" longname="记账交易类型" leftPadding="false" ref="DpRiskDict.JfDp.jftrtp"/>
            <field id="inmeid" type="BaseType.U_CHAR25" longname="内部商户号" leftPadding="false" ref="DpRiskDict.JfDp.inmeid"/>
