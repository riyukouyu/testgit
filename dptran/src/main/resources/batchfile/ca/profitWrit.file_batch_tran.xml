<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="profitWrit" longname="分润记账写文件" kind="write" package="cn.sunline.edsp.busi.dptran.batchfile.ca" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <fileTemplate id="profitWrit" longname="分润记账写文件" mode="splitor" splitor="|" encoding="UTF-8">
