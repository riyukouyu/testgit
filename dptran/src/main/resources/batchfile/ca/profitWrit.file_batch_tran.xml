            <field id="crcycd" type="BaseType.U_CRCYCD" longname="币种" leftPadding="false" ref="BaseDict.Comm.crcycd"/>
            <field id="transt" type="BaseEnumType.E_TRANST" longname="交易状态" leftPadding="false" ref="PbDict.Cash.transt"/>
            <field id="errsck" type="KBaseType.U_CUOWDUIZ" longname="错误堆栈" leftPadding="false" ref="SPBatCfDict.BtDict.errsck"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
