            <field id="crcycd" type="BaseType.U_CRCYCD" longname="币种" leftPadding="false" ref="BaseDict.Comm.crcycd"/>
            <field id="transt" type="BaseEnumType.E_TRANST" longname="交易状态" leftPadding="false"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.edsp.busi.dptran.batchfile.ca.intf">
        <input packMode="true"/>
        <output asParm="true" packMode="true"/>
        <property packMode="true">
            <field id="lastdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="上次交易日期" ref="BaseDict.Comm.lastdt"/>
        </property>
        <printer packMode="true"/>
    </interface>
