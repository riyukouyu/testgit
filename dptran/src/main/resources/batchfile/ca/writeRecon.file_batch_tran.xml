<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="writeRecon" longname="写对账文件" kind="write" package="cn.sunline.edsp.busi.dptran.batchfile.ca" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <fileTemplate id="writeRecon" longname="写对账文件" mode="splitor" splitor="|" encoding="UTF-8">
        <header id="Header" longname="文件头" startline="1"/>
        <body id="Body" longname="文件体" mapping="DpAccount.KnsAcsqRgst" startline="1">
            <field id="mntrsq" type="BaseType.U_SEQUNO" longname="主交易流水" leftPadding="false" ref="BaseDict.Comm.mntrsq"/>
            <field id="mntrdt" type="BaseType.U_DATE08" longname="交易日期" leftPadding="false" ref="DpRiskDict.JfDp.mntrdt"/>
            <field id="cltrsq" type="BaseType.U_CHAR32" longname="清结算流水" leftPadding="false" ref="DpRiskDict.JfDp.cltrsq"/>
            <field id="cltrdt" type="BaseType.U_DATE08" longname="清结算日期" leftPadding="false" ref="DpRiskDict.JfDp.cltrdt"/>
