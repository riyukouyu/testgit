<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="rdopac" longname="读取批量开户文件" kind="read" package="cn.sunline.ltts.busi.catran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[读取批量开户文件]]></description>
    <fileTemplate id="rdopac" longname="读物批量开户文件" mode="splitor" splitor="|+|" encoding="UTF-8">
        <header id="Header" longname="文件头" startline="1">
            <field id="agntna" type="BaseType.U_CUSTNA" longname="代理单位名称" leftPadding="false" mode="splitor"/>
            <field id="aghmna" type="BaseType.U_CUSTNA" longname="代理人名称" leftPadding="false" mode="splitor"/>
            <field id="agidtp" type="BaseEnumType.E_IDTFTP" longname="代理人证件类型" leftPadding="false" mode="splitor"/>
            <field id="agidno" type="BaseType.U_IDTFNO" longname="代理人证件号码" leftPadding="false" mode="splitor"/>
            <field id="accatp" type="BaseEnumType.E_ACCATP" longname="账户分类" leftPadding="false" mode="splitor"/>
            <field id="brchno" type="BaseType.U_BRCHNO" longname="归属机构" leftPadding="false" mode="splitor"/>
            <field id="servtp" type="MsType.U_CHANNEL" longname="交易渠道" leftPadding="false" mode="splitor"/>
            <field id="counts" type="BaseType.U_LONGIN" longname="总记录数" leftPadding="false" mode="splitor"/>
            <field id="tranus" type="BaseType.U_USERID" longname="操作柜员" leftPadding="false" mode="splitor"/>
        </header>
        <body id="Body" longname="文件体" mapping="ElectronicAccount.KnbOpbt" startline="2">
            <field id="custid" type="BaseType.U_CUSTID" longname="用户ID" leftPadding="false" ref="BaseDict.Comm.custid" mode="splitor"/>
            <field id="custna" type="BaseType.U_ACCTNA" longname="客户名称" leftPadding="false" ref="DpDict.Acct.custna" mode="splitor"/>
            <field id="idtftp" type="BaseType.U_CHAR20" longname="证件类型" leftPadding="false" mode="splitor"/>
            <field id="idtfno" type="BaseType.U_IDTFNO" longname="证件号码" leftPadding="false" ref="DpDict.Acct.idtfno" mode="splitor"/>
            <field id="teleno" type="BaseType.U_TELENO" longname="电话号码" leftPadding="false" ref="DpDict.Acct.teleno" mode="splitor"/>
            <field id="idckrt" type="CaEnumType.E_IDCKRT" longname="身份核查结果" leftPadding="false" ref="CaDict.Eacct.idckrt" mode="splitor"/>
            <field id="mpckrt" type="CaEnumType.E_MPCKRT" longname="人脸识别结果" leftPadding="false" ref="CaDict.Eacct.mpckrt" mode="splitor"/>
            <field id="custcd" type="BaseType.U_CHAR20" longname="客户内码" leftPadding="false" ref="BaseDict.Comm.custcd" mode="splitor"/>
            <field id="crcycd" type="BaseType.U_CRCYCD" longname="币种" leftPadding="false" ref="BaseDict.Comm.crcycd" mode="splitor"/>
            <field id="openrt" type="CaEnumType.E_OPENRT" longname="开户结果" leftPadding="false" ref="CaDict.Eacct.openrt" mode="splitor"/>
            <field id="losern" type="BaseType.U_SHORDS" longname="失败原因" leftPadding="false" ref="CaDict.Eacct.losern" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.catran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="当前批次号"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="agntna" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="代理单位名称"/>
            <field id="accatp" type="BaseEnumType.E_ACCATP" required="false" multi="false" array="false" longname="账户分类"/>
            <field id="brchno" type="BaseType.U_BRCHNO" required="false" multi="false" array="false" longname="归属机构"/>
            <field id="tranus" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="操作柜员"/>
            <field id="servtp" type="MsType.U_CHANNEL" required="false" multi="false" array="false" longname="交易渠道"/>
            <field id="aghmna" type="BaseType.U_CUSTNA" required="false" multi="false" array="false" longname="代理人名称"/>
            <field id="agidtp" type="BaseEnumType.E_IDTFTP" required="false" multi="false" array="false" longname="代理人证件种类"/>
            <field id="agidno" type="BaseType.U_IDTFNO" required="false" multi="false" array="false" longname="代理人证件号码"/>
        </property>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
