<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="rdckrt" longname="读取身份核查对账文件" kind="read" package="cn.sunline.ltts.busi.catran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
