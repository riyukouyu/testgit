<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<file_batch_transaction id="payckawt" longname="支付2.0对账文件" kind="write" package="cn.sunline.ltts.busi.dptran.batchfile" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <fileTemplate id="paycka" longname="支付2.0对账文件" mode="splitor" splitor="|" encoding="UTF-8">
        <header id="Header" longname="文件头" startline="1"/>
        <body id="Body" longname="文件体" mapping="DpDepoInst.KnbPaycBatch" startline="1">
            <field id="otcustac" type="BaseType.U_CUSTAC" longname="付款账号" leftPadding="false" mode="splitor"/>
            <field id="otcustna" type="BaseType.U_ACCTNA" longname="付款户名" leftPadding="false" mode="splitor"/>
            <field id="incustac" type="BaseType.U_CUSTAC" longname="收款账号" leftPadding="false" mode="splitor"/>
            <field id="incustna" type="BaseType.U_ACCTNA" longname="收款户名" leftPadding="false" mode="splitor"/>
            <field id="tranam" type="PbBaseType.U_DEC172" longname="交易金额" leftPadding="false" ref="PbDict.Intr.tranam" mode="splitor"/>
            <field id="servsq" type="BaseType.U_SEQUNO" longname="渠道流水" leftPadding="false" ref="DpDict.Acct.servsq" mode="splitor"/>
            <field id="servdt" type="BaseType.U_DATE08" longname="渠道日期" leftPadding="false" mode="splitor"/>
            <field id="transq" type="BaseType.U_SEQUNO" longname="交易流水" leftPadding="false" ref="PbDict.Charge.transq" mode="splitor"/>
            <field id="keepdt" type="BaseType.U_DATE08" longname="清算日期" leftPadding="false" mode="splitor"/>
            <field id="status" type="BaseEnumType.E_TRANST" longname="交易状态" leftPadding="false" mode="splitor"/>
            <field id="capitp" type="DpEnumType.E_CAPITP" longname="交易描述" leftPadding="false" mode="splitor"/>
            <field id="ioflag" type="DpEnumType.E_IOFLAG" longname="出入金标志" leftPadding="false" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="文件批次号"/>
            <field id="chckdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="文件日期"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true">
            <field id="neibjlid" type="long" required="false" multi="false" array="false" longname="内部处理ID"/>
            <field id="taskid" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="批量交易批次号" ref="ApDict.Aplt.taskid"/>
            <field id="fhwjmche" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="回盘文件名称"/>
            <field id="fhwjbdlj" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="回盘文件目录"/>
            <field id="shujubsid" type="ApBaseType.U_LEIXIN80" required="false" multi="false" array="false" longname="数据标识ID"/>
            <field id="chekdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="文件日期"/>
        </property>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
