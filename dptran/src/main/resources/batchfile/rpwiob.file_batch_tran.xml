            <field id="proccd" type="BaseType.U_CHAR06" longname="处理码" leftPadding="false" mode="splitor"/>
            <field id="prdate" type="BaseType.U_DATE08" longname="前置交易日期" leftPadding="false" mode="splitor"/>
            <field id="prbrmk" type="BaseType.U_CHAR16" longname="代理机构标识号" leftPadding="false" mode="splitor"/>
            <field id="trbrmk" type="BaseType.U_CHAR16" longname="发送机构标识号" leftPadding="false" mode="splitor"/>
            <field id="spared" type="BaseType.U_CHAR20" longname="备用" leftPadding="false" mode="splitor"/>
        </body>
        <foot id="Foot" longname="文件尾" startline="1"/>
    </fileTemplate>
    <interface package="cn.sunline.ltts.busi.dptran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="true" multi="false" array="false" longname="批量交易批次号"/>
            <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="对账日期"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true"/>
        <printer packMode="false"/>
    </interface>
</file_batch_transaction>
