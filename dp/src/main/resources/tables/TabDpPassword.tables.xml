<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<schema id="TabDpPassword" package="cn.sunline.ltts.busi.dp.tables" longname="交易密码相关表" classgen="normal" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[交易密码单独管理，原在卡表中的交易密码作废]]></description>
    <table id="DpbPswd" name="dpb_pswd" longname="密码信息表" virtual="false" extension="SysCommFieldTable.KapComm" abstract="false">
        <fields>
            <field id="acctno" longname="账号" type="BaseType.U_ACCTNO" ref="BaseDict.Comm.acctno" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="servno" longname="交易渠道号" type="BaseType.U_CHAR50" ref="BaseDict.Comm.servno" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="servtp" longname="渠道" type="MsType.U_CHANNEL" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="passwd" longname="交易密码" type="BaseType.U_TRANPW" ref="ApDict.Aplt.passwd" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="acctrt" longname="账号路由类型" type="BaseEnumType.E_ACCTROUTTYPE" ref="BaseDict.Comm.acctrt" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="pwerct" longname="密码错误次数" type="BaseType.U_INTEGE" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="maxerr" longname="最大密码错误次数" type="BaseType.U_INTEGE" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="psdwst" longname="状态" type="BaseEnumType.E_PSDWST" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
        </fields>
        <ddls/>
        <odbindexes>
            <index id="odb1" type="unique" fields="acctno servno servtp" operate="selectOne updateOne selectOneWithLock"/>
        </odbindexes>
        <indexes>
            <index id="dpb_pswd_uk1" type="unique" fields="acctno servno servtp corpno" longname="主键唯一索引">
                <ddls/>
            </index>
        </indexes>
    </table>
    <table id="DpsPswd" name="dps_pswd" longname="密码维护交易表" virtual="false" extension="SysCommFieldTable.KapComm" abstract="false">
        <fields>
            <field id="transq" longname="交易流水" type="BaseType.U_SEQUNO" ref="BaseDict.Comm.transq" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="trandt" longname="交易日期" type="BaseType.U_DATE08" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="acctno" longname="账号" type="BaseType.U_ACCTNO" ref="BaseDict.Comm.acctno" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="servno" longname="交易渠道号" type="BaseType.U_CHAR50" ref="BaseDict.Comm.servno" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="servtp" longname="渠道" type="MsType.U_CHANNEL" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="acctrt" longname="账号路由类型" type="BaseEnumType.E_ACCTROUTTYPE" ref="BaseDict.Comm.acctrt" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="prcscd" longname="交易码" type="BaseType.U_PRCSCD" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
            <field id="tranus" longname="交易柜员" type="BaseType.U_USERID" ref="BaseDict.Comm.tranus" primarykey="false" final="false" nullable="true" identity="false" allowSubType="true"/>
        </fields>
        <ddls/>
        <odbindexes>
            <index id="odb1" type="unique" fields="transq" operate="selectOne"/>
        </odbindexes>
        <indexes>
            <index id="dps_pswd_uk1" type="unique" fields="transq corpno" longname="主键唯一索引">
                <ddls/>
            </index>
        </indexes>
    </table>
</schema>
