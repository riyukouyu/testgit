<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="KnaAcctService" kind="business" longname="查询电子子账户信息列表" package="cn.sunline.edsp.busi.dp.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <service id="IQuerySubAcctInfo" name="IQuerySubAcctInfo" longname="查询电子子账户信息列表">
        <interface>
            <input packMode="true">
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="负债账号" ref="DpDict.Acct.acctno"/>
                <field id="custno" type="BaseType.U_CUSTNO" required="false" multi="false" array="false" longname="客户号" ref="DpDict.Acct.custno"/>
                <field id="acctna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="子账户名称" ref="DpDict.Acct.acctna"/>
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.cardno"/>
                <field id="acctid" type="BaseType.U_CHAR30" required="false" multi="false" array="false" longname="子账户开户标识" ref="DpDict.Acct.acctid"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="subAcctInfoList" type="DpAcctType.AcctSbad" required="false" multi="true" array="false" longname="电子子账户信息列表"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IQuerySubAcctId" name="IQuerySubAcctId" longname="查询电子主账户所有子户标识">
        <interface>
            <input packMode="true">
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="客户账号" ref="DpDict.Acct.cardno"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="subList" type="DpAcctType.AcctSub" required="false" multi="true" array="false" longname="子户信息"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
    <service id="IQueryAcctNbl" name="queryAcctOnlNbl" longname="查询电子账户当前余额">
        <interface>
            <input packMode="true">
                <field id="inmeid" type="BaseType.U_CHAR25" required="false" multi="false" array="false" longname="内部商户ID"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="onlnbl" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="当前账户余额" ref="DpDict.Acct.onlnbl"/>
                <field id="lowBalance" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="低风险余额"/>
                <field id="highBalance" type="BaseType.U_ACBLTP" required="false" multi="false" array="false" longname="高风险余额"/>
            </output>
            <property packMode="false"/>
            <printer packMode="true"/>
        </interface>
    </service>
</serviceType>
