    <interface package="cn.sunline.ltts.busi.intran.batchfile.intf">
        <input packMode="true">
            <field id="filesq" type="BaseType.U_CHAR40" required="false" multi="false" array="false" longname="文件批次号"/>
            <field id="trandt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="文件日期"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="true" />
        <printer packMode="false"/>
    </interface>
