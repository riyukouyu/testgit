<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="iacuql" longname="内部账转客户账查询" kind="1" package="cn.sunline.ltts.busi.intran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
