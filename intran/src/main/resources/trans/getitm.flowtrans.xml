<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="getitm" longname="查询科目信息" kind="query" package="cn.sunline.ltts.busi.intran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[查询科目信息]]></description>
    <interface package="cn.sunline.ltts.busi.intran.trans.intf">
        <input packMode="false">
            <field id="startid" type="integer" required="false" multi="false" array="false" longname="起始记录"/>
            <field id="recdct" type="integer" required="false" multi="false" array="false" longname="页容量"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="redata" scope="InItemInfo.ItemInfos" required="false" multi="false" longname="科目信息" array="false">
                <field id="count" type="integer" required="false" multi="false" array="false" longname="总记录数 "/>
                <field id="itinfo" type="InItemInfo.ItemInfo" required="false" multi="true" array="false" longname="科目信息集合"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="ItemType.QryItemInfo" id="QryItemInfo" longname="分页查询科目信息" desc="分页查询科目信息">
            <in_mappings by_interface="true">
                <mapping src="recdct" dest="recdct" by_interface="true" on_top="true"/>
                <mapping src="startid" dest="startid" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="itemInfo.count" dest="redata.count" by_interface="true" on_top="true"/>
                <mapping src="itemInfo.itinfo" dest="redata.itinfo" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
