<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" id="inqbil" longname="内部户交易明细分页查询" kind="1" package="cn.sunline.ltts.busi.intran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd">
    <interface package="cn.sunline.ltts.busi.intran.trans.intf">
        <input packMode="false">
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="内部户账号" ref="InDict.inac.acctno"/>
            <field id="bgdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="开始日期"/>
            <field id="endate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="结束日期"/>
            <field id="jranus" type="BaseType.U_USERID" required="false" multi="false" array="false" longname="记账柜员"/>
            <field id="toacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="对方账号" ref="InDict.inac.toacct"/>
        </input>
        <output asParm="false" packMode="true">
            <field id="inBillInfo" type="InQueryTypes.InBillInfo" required="false" multi="true" array="false" longname="内部户交易明细"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="UpInacInfo.selBillInfo" id="selBillInfo" longname="内部户明细分页查询" desc="内部户明细分页查询">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="bgdate" dest="bgdate" by_interface="true" on_top="true"/>
                <mapping src="endate" dest="endate" by_interface="true" on_top="true"/>
                <mapping src="#run.page_start" dest="pageno" by_interface="true" on_top="true"/>
                <mapping src="#run.page_size" dest="pagesize" by_interface="true" on_top="true"/>
                <mapping src="toacct" dest="toacct" by_interface="true" on_top="true"/>
                <mapping src="jranus" dest="tranus" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="false">
                <mapping src="bInfos.billInfos" dest="inBillInfo" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
