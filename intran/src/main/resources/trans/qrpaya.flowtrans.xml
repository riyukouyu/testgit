<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrpaya" longname="查询挂账明细登记簿" kind="1" package="cn.sunline.ltts.busi.intran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.intran.trans.intf">
        <input packMode="false">
            <field id="payasq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="待销账流水" ref="InDict.inac.payasq"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="挂账账号"/>
            <field id="status" type="InEnumType.E_PAQRST" required="false" multi="false" array="false" longname="挂账状态"/>
            <field id="bgdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始日期"/>
            <field id="endate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="截止日期"/>
        </input>
        <output asParm="true" packMode="false">
            <field id="payaInfo" type="InAcctTranOut.PayaListOutPut" required="false" multi="true" array="false" longname="挂账登记簿明细"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoInAcctTranOut.SelPayaList" id="SelPayaList" longname="查询挂账登记簿明细">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="bgdate" dest="bgdate" by_interface="true" on_top="true"/>
                <mapping src="endate" dest="endate" by_interface="true" on_top="true"/>
                <mapping src="payasq" dest="payasq" by_interface="true" on_top="true"/>
                <mapping src="status" dest="status" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="payaInfo" dest="payaInfo" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
    <outMapping/>
    <propertyToPrinterMapping/>
    <outToPrinterMapping/>
</flowtran>
