<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrpayd" longname="查询销账明细登记簿" kind="1" package="cn.sunline.ltts.busi.intran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.intran.trans.intf">
        <input packMode="false">
            <field id="payasq" type="BaseType.U_SEQUNO" required="false" multi="false" array="false" longname="待销账流水" ref="InDict.inac.payasq"/>
            <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="挂账账号"/>
            <field id="status" type="InEnumType.E_PDQRST" required="false" multi="false" array="false" longname="销账状态"/>
            <field id="bgdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="起始日期"/>
            <field id="endate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="截止日期"/>
            <field id="toacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="对方账号" ref="InDict.inac.toacct"/>
            <field id="paydsq" type="BaseType.U_PAYASQ" required="false" multi="false" array="false" longname="销账序号" ref="InDict.inac.paydsq"/>
        </input>
        <output asParm="false" packMode="false">
            <field id="paydInfo" type="InAcctTranOut.PaydListOutPut" required="false" multi="true" array="false" longname="销账登记簿明细输出"/>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoInAcctTranOut.SelPaydList" id="SelPaydList" longname="查询销账登记簿信息">
            <in_mappings by_interface="true">
                <mapping src="acctno" dest="acctno" by_interface="true" on_top="true"/>
                <mapping src="bgdate" dest="bgdate" by_interface="true" on_top="true"/>
                <mapping src="endate" dest="endate" by_interface="true" on_top="true"/>
                <mapping src="payasq" dest="payasq" by_interface="true" on_top="true"/>
                <mapping src="paydsq" dest="paydsq" by_interface="true" on_top="true"/>
                <mapping src="status" dest="status" by_interface="true" on_top="true"/>
                <mapping src="toacct" dest="toacct" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="paydInfo" dest="paydInfo" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
