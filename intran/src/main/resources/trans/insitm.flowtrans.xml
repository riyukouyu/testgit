                <mapping src="busino" dest="inputItem.busino" by_interface="true" on_top="true"/>
                <mapping src="dtittg" dest="inputItem.dtittg" by_interface="true" on_top="true"/>
                <mapping src="ioflag" dest="inputItem.ioflag" by_interface="true" on_top="true"/>
                <mapping src="itemcd" dest="inputItem.itemcd" by_interface="true" on_top="true"/>
                <mapping src="itemlv" dest="inputItem.itemlv" by_interface="true" on_top="true"/>
                <mapping src="itemna" dest="inputItem.itemna" by_interface="true" on_top="true"/>
                <mapping src="itemrg" dest="inputItem.itemrg" by_interface="true" on_top="true"/>
                <mapping src="itemtp" dest="inputItem.itemtp" by_interface="true" on_top="true"/>
                <mapping src="ittype" dest="inputItem.ittype" by_interface="true" on_top="true"/>
