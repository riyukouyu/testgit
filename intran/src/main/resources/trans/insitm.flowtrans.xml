            <field id="ittype" type="InEnumType.E_ITTYPE" required="false" multi="false" array="false" longname="科目类别" ref="InDict.inac.ittype"/>
            <field id="proftp" type="InEnumType.E_PROFTP" required="false" multi="false" array="false" longname="损益类别" ref="InDict.inac.proftp"/>
            <field id="busino" type="BaseType.U_BUSINO" required="false" multi="false" array="false" longname="业务代码" ref="InDict.inac.busino"/>
        </input>
        <output asParm="true" packMode="false"/>
        <property packMode="false"/>
        <printer packMode="false"/>
