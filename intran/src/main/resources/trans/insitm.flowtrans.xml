            <field id="dtittg" type="BaseEnumType.E_YES___" required="false" multi="false" array="false" longname="是否明细科目" ref="InDict.inac.dtittg"/>
            <field id="blncdn" type="InEnumType.E_BLNCDN" required="false" multi="false" array="false" longname="余额方向CDR" ref="InDict.inac.blncdn"/>
            <field id="ioflag" type="InEnumType.E_IOFLAG" required="false" multi="false" array="false" longname="表内外标志" ref="InDict.inac.ioflag"/>
            <field id="upitem" type="BaseType.U_ITEMCD" required="false" multi="false" array="false" longname="上级科目号" ref="InDict.inac.upitem"/>
