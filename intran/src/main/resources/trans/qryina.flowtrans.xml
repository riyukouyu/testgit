<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qryina" longname="分页查询内部户信息" kind="query" package="cn.sunline.ltts.busi.intran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[分页查询内部户信息]]></description>
    <interface package="cn.sunline.ltts.busi.intran.trans.intf">
        <input packMode="false">
            <field id="start" type="int" required="true" multi="false" array="false" longname="开始记录"/>
            <field id="pagect" type="int" required="true" multi="false" array="false" longname="页记录数"/>
        </input>
        <output asParm="true" packMode="true">
            <fields id="redata" scope="IoInQueryTypes.InacInfoList" required="false" multi="false" longname="内部户信息" array="false">
                <field id="count" type="integer" required="false" multi="false" array="false" longname="当前记录数"/>
                <field id="infos" type="IoInQueryTypes.IoInacInfo" required="false" multi="true" array="false" longname="内部户信息"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoInQuery.SelAllInacInfo" id="selAllInacInfo" longname="分页查询内部户信息" desc="分页查询内部户信息">
            <in_mappings by_interface="true">
                <mapping src="pagect" dest="pagect" by_interface="true" on_top="true"/>
                <mapping src="start" dest="start" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="inacInfo.count" dest="redata.count" by_interface="true" on_top="true"/>
                <mapping src="inacInfo.infos" dest="redata.infos" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
