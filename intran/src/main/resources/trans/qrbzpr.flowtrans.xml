<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<flowtran id="qrbzpr" longname="分页查询业务代码属性信息" kind="account" package="cn.sunline.ltts.busi.intran.trans" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <interface package="cn.sunline.ltts.busi.intran.trans.intf">
        <input packMode="false">
            <field id="starno" type="integer" required="false" multi="false" array="false" longname="起始记录"/>
            <field id="pageno" type="integer" required="false" multi="false" array="false" longname="页记录数"/>
        </input>
        <output asParm="true" packMode="false">
            <fields id="bzprtpInfoList" scope="IoInQueryTypes.IoBzprtpAllInfoList" required="false" multi="false" longname="属性类型信息列表" array="false">
                <field id="count" type="integer" required="false" multi="false" array="false" longname="记录总数"/>
                <field id="bzprtpInfo" type="IoInQueryTypes.IoBzprtpAllInfo" required="false" multi="true" array="false" longname="属性类型信息"/>
            </fields>
        </output>
        <property packMode="false"/>
        <printer packMode="false"/>
    </interface>
    <flow>
        <service serviceName="IoInQuery.IoSelBzprtpInfoWithPage" id="selBzprtpInfoWithPage" longname="分页查询属性类型信息" desc="分页查询属性类型信息">
            <in_mappings by_interface="true">
                <mapping src="pageno" dest="pageno" desc="页记录数" by_interface="true" on_top="true"/>
                <mapping src="starno" dest="starno" desc="起始记录" by_interface="true" on_top="true"/>
            </in_mappings>
            <out_mappings by_interface="true">
                <mapping src="bzprtpInfos.bzprtpInfo" dest="bzprtpInfoList.bzprtpInfo" desc="属性类型信息" by_interface="true" on_top="true"/>
                <mapping src="bzprtpInfos.count" dest="bzprtpInfoList.count" desc="总记录数" by_interface="true" on_top="true"/>
            </out_mappings>
        </service>
    </flow>
</flowtran>
