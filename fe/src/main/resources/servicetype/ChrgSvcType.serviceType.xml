<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serviceType id="ChrgSvcType" kind="business" longname="收费内部服务" package="cn.sunline.ltts.busi.cg.servicetype" xsi:noNamespaceSchemaLocation="ltts-model.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <description><![CDATA[包含按事件收费，收费代码收费，欠费收费，场景收费，计费视图]]></description>
    <service id="publicCharge" name="publicCharge" longname="公共收费">
        <interface>
            <input packMode="false">
                <field id="input" type="CgComplexType.PublicCharge_IN" required="false" multi="false" array="false" longname="公共收费输入"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="chargeInfo" type="CgComplexType.ChargeInfo" required="false" multi="true" array="false" longname="汇总销记登记簿明细"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="SelChargRegistGroupByTrifno" name="selChargRegistGroupByTrifno" longname="查询集中收费登记簿">
        <interface>
            <input packMode="false">
                <field id="bgdate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="开始时间" ref="BaseDict.Comm.bgdate"/>
                <field id="endate" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="结束时间" ref="BaseDict.Comm.endate"/>
                <field id="acctno" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="负债帐号"/>
                <field id="cardno" type="BaseType.U_CARDNO" required="false" multi="false" array="false" longname="卡号"/>
            </input>
            <output asParm="true" packMode="true">
                <field id="chargeInfo" type="CgComplexType.ChargeInfo" required="false" multi="true" array="false" longname="汇总销记登记簿明细"/>
            </output>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
    <service id="chargeAdjust" name="chargeAdjust" longname="收费调整">
        <interface>
            <input packMode="true">
                <field id="prtrdt" type="BaseType.U_DATE08" required="false" multi="false" array="false" longname="原交易日期"/>
                <field id="prtrsq" type="PbBaseType.U_TRNSEQ" required="false" multi="false" array="false" longname="原交易流水"/>
                <field id="cgpyrv" type="PbEnumType.E_CGPYRV" required="false" multi="false" array="false" longname="费用收付标志" ref="PbDict.Charge.cgpyrv"/>
                <field id="evrgsq" type="PbBaseType.U_SENOTP" required="false" multi="false" array="false" longname="事件登记序号" ref="PbDict.Charge.evrgsq"/>
                <field id="chrgcd" type="PbBaseType.U_CHRGCD" required="false" multi="false" array="false" longname="收费代码" ref="PbDict.Charge.chrgcd"/>
                <field id="chrgna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="收费代码名称" ref="PbDict.Charge.chrgna"/>
                <field id="scencd" type="PbBaseType.U_SCENCD" required="false" multi="false" array="false" longname="场景代码" ref="PbDict.Charge.scencd"/>
                <field id="scends" type="PbBaseType.U_LONGDS" required="false" multi="false" array="false" longname="场景描述" ref="PbDict.Charge.scends"/>
                <field id="trinfo" type="BaseType.U_CHAR10" required="false" multi="false" array="false" longname="交易信息" ref="PbDict.Dcmt.trinfo"/>
                <field id="prodcd" type="BaseType.U_CHAR32" required="false" multi="false" array="false" longname="产品代码" ref="PbDict.Dcmt.prodcd"/>
                <field id="toacct" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="对方账号"/>
                <field id="crcycd" type="BaseType.U_CRCYCD" required="false" multi="false" array="false" longname="币种"/>
                <field id="prtram" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="金额"/>
                <field id="adjttp" type="PbEnumType.E_ADJTTP" required="false" multi="false" array="false" longname="收费调整方式" ref="PbDict.Charge.adjttp"/>
                <field id="jtchcd" type="PbBaseType.U_CHRGCD" required="false" multi="false" array="false" longname="调整后费种代码"/>
                <field id="jtchna" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="调整后费种名称"/>
                <field id="jtsccd" type="PbBaseType.U_SCENCD" required="false" multi="false" array="false" longname="调整后场景代码"/>
                <field id="jtscds" type="PbBaseType.U_LONGDS" required="false" multi="false" array="false" longname="调整后场景名称"/>
                <field id="jttrif" type="BaseType.U_CHAR10" required="false" multi="false" array="false" longname="调整后交易信息"/>
                <field id="jtprod" type="BaseType.U_PRODCD" required="false" multi="false" array="false" longname="调整后产品编码"/>
                <field id="adjtam" type="BaseType.U_TRANAM" required="false" multi="false" array="false" longname="调整金额"/>
                <field id="acctno" type="BaseType.U_ACCTNO" required="false" multi="false" array="false" longname="账号"/>
                <field id="acctna" type="BaseType.U_ACCTNA" required="false" multi="false" array="false" longname="户名"/>
                <field id="remark" type="BaseType.U_CHAR80" required="false" multi="false" array="false" longname="调整原因"/>
                <field id="payalst" type="InacTransComplex.IaPayaDetail" required="false" multi="true" array="false" longname="挂账信息列表"/>
                <field id="paydlst" type="InacTransComplex.IaPaydDetail" required="false" multi="true" array="false" longname="销账信息列表"/>
            </input>
            <output asParm="true" packMode="false"/>
            <property packMode="false"/>
            <printer packMode="false"/>
        </interface>
    </service>
</serviceType>
